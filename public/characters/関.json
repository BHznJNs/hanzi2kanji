  "sentences": [
    {
      "content": "私には関係ない。",
      "furigana": "[私|わたし]には[関係|かん|けい]ない。",
      "translation_zh_CN": "这件事和我没有关系。"
    },
    {
      "content": "だれか玄関にいる。",
      "furigana": "だれか[玄関|げん|かん]にいる。",
      "translation_zh_CN": "有人在门口。"
    },
    {
      "content": "お前には関係ない。",
      "furigana": "お[前|まえ]には[関係|かん|けい]ない。",
      "translation_zh_CN": "这不关你的事。"
    },
    {
      "content": "政治に関心があるの？",
      "furigana": "[政治|せい|じ]に[関心|かん|しん]があるの？",
      "translation_zh_CN": "你对政治感兴趣吗？"
    },
    {
      "content": "玄関に誰かいるよ。",
      "furigana": "[玄関|げん|かん]に[誰|だれ]かいるよ。",
      "translation_zh_CN": "有人在门口。"
    },
    {
      "content": "あなたには関係ない。",
      "furigana": "あなたには[関係|かん|けい]ない。",
      "translation_zh_CN": "这不关你的事。"
    },
    {
      "content": "両者の間に関係はない。",
      "furigana": "[両者|りょう|しゃ]の[間|ま]に[関係|かん|けい]はない。",
      "translation_zh_CN": "两者没有关联。"
    },
    {
      "content": "私はそれには関係ない。",
      "furigana": "[私|わたし]はそれには[関係|かん|けい]ない。",
      "translation_zh_CN": "我和这件事无关。"
    },
    {
      "content": "学ぶのに年は関係ない。",
      "furigana": "[学|まな]ぶのに[年|とし]は[関係|かん|けい]ない。",
      "translation_zh_CN": "活到老，学到老。"
    },
    {
      "content": "だれか玄関に来てるよ。",
      "furigana": "だれか[玄関|げん|かん]に[来|き]てるよ。",
      "translation_zh_CN": "玄关有人来了耶。"
