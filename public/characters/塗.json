  "sentences": [
    {
      "content": "ペンキ塗りたて注意",
      "furigana": "ペンキ[塗|ぬ]りたて[注意|ちゅう|い]",
      "translation_zh_CN": "注意！油漆未干"
    },
    {
      "content": "日焼け止め塗りな。",
      "furigana": "[日焼|ひ|や]け[止|ど]め[塗|ぬ]りな。",
      "translation_zh_CN": "涂点防晒吧。"
    },
    {
      "content": "彼女は壁を白く塗った。",
      "furigana": "[彼女|かの|じょ]は[壁|かべ]を[白|しろ]く[塗|ぬ]った。",
      "translation_zh_CN": "她把墙壁都涂成了白色。"
    },
    {
      "content": "彼は天井を青く塗った。",
      "furigana": "[彼|かれ]は[天井|てん|じょう]を[青|あお]く[塗|ぬ]った。",
      "translation_zh_CN": "他把天花板刷成了蓝色。"
    },
    {
      "content": "ペンキを塗らなくちゃ。",
      "furigana": "ペンキを[塗|ぬ]らなくちゃ。",
      "translation_zh_CN": "我必须把它漆上油漆。"
    },
    {
      "content": "パンにチーズを塗った。",
      "furigana": "パンにチーズを[塗|ぬ]った。",
      "translation_zh_CN": "我把起司涂在面包上"
    },
    {
      "content": "彼は自転車を赤く塗った。",
      "furigana": "[彼|かれ]は[自転車|じ|てん|しゃ]を[赤|あか]く[塗|ぬ]った。",
      "translation_zh_CN": "他把他的自行车刷成红色。"
    },
    {
      "content": "壁にペンキを塗るんです。",
      "furigana": "[壁|かべ]にペンキを[塗|ぬ]るんです。",
      "translation_zh_CN": "我们准备刷墙。"
    },
    {
      "content": "彼らは窓枠を黄色く塗った。",
      "furigana": "[彼|かれ]らは[窓|まど][枠|わく]を[黄色|き|いろ]く[塗|ぬ]った。",
      "translation_zh_CN": "她们把窗框涂成了黄色。"
    },
    {
      "content": "私はフェンスを緑に塗った。",
      "furigana": "[私|わたし]はフェンスを[緑|みどり]に[塗|ぬ]った。",
      "translation_zh_CN": "我把篱笆漆成了绿色。"
    }
  ],
