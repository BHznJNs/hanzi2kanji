  "sentences": [
    {
      "content": "離せっつってんだろ！",
      "furigana": "[離|はな]せっつってんだろ！",
      "translation_zh_CN": "你是在告诉我放手吧！"
    },
    {
      "content": "彼女は夫と離婚した。",
      "furigana": "[彼女|かの|じょ]は[夫|おっと]と[離婚|り|こん]した。",
      "translation_zh_CN": "她与丈夫离婚。"
    },
    {
      "content": "彼は親離れしている。",
      "furigana": "[彼|かれ]は[親|おや][離|ばな]れしている。",
      "translation_zh_CN": "他独立于他的父母。"
    },
    {
      "content": "親が離婚しています。",
      "furigana": "[親|おや]が[離婚|り|こん]しています。",
      "translation_zh_CN": "我父母离婚了。"
    },
    {
      "content": "両親は離婚しました。",
      "furigana": "[両親|りょう|しん]は[離婚|り|こん]しました。",
      "translation_zh_CN": "我父母离婚了。"
    },
    {
      "content": "飛行機は離陸間際だ。",
      "furigana": "[飛行機|ひ|こう|き]は[離陸|り|りく][間際|ま|ぎわ]だ。",
      "translation_zh_CN": "飞机快要起飞了。"
    },
    {
      "content": "距離が判断できません。",
      "furigana": "[距離|きょ|り]が[判断|はん|だん]できません。",
      "translation_zh_CN": "我无法判断距离。"
    },
    {
      "content": "今仕事から手が離せない。",
      "furigana": "[今|こん][仕事|し|ごと]から[手|て]が[離|はな]せない。",
      "translation_zh_CN": "我现在上班无法离开。"
    },
    {
      "content": "駅までは距離があります。",
      "furigana": "[駅|えき]までは[距離|きょ|り]があります。",
      "translation_zh_CN": "到车站的路程很远。"
    },
    {
      "content": "離婚するんじゃないかしら。",
      "furigana": "[離婚|り|こん]するんじゃないかしら。",
      "translation_zh_CN": "我想她会和他离婚。"
    },
    {
      "content": "彼は先月奥さんと離婚した。",
      "furigana": "[彼|かれ]は[先月|せん|げつ][奥|おく]さんと[離婚|り|こん]した。",
      "translation_zh_CN": "上个月他和妻子离婚了。"
    }
