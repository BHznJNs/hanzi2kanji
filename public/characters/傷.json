  "sentences": [
    {
      "content": "傷がふさがりません。",
      "furigana": "[傷|きず]がふさがりません。",
      "translation_zh_CN": "伤口不会闭合。"
    },
    {
      "content": "傷が化膿しました。",
      "furigana": "[傷|きず]が[化膿|か|のう]しました。",
      "translation_zh_CN": "伤口溃烂。"
    },
    {
      "content": "傷がうずいていた。",
      "furigana": "[傷|きず]がうずいていた。",
      "translation_zh_CN": "伤口很痛。"
    },
    {
      "content": "傷を見せてください。",
      "furigana": "[傷|きず]を[見|み]せてください。",
      "translation_zh_CN": "请让我看看你的伤疤。"
    },
    {
      "content": "トムは傷を負った。",
      "furigana": "トムは[傷|きず]を[負|お]った。",
      "translation_zh_CN": "汤姆受伤了。"
    },
    {
      "content": "傷はまだ治っていない。",
      "furigana": "[傷|きず]はまだ[治|なお]っていない。",
      "translation_zh_CN": "伤口还没有愈合。"
    },
    {
      "content": "傷から流れる血を止めなさい。",
      "furigana": "[傷|きず]から[流|なが]れる[血|ち]を[止|と]めなさい。",
      "translation_zh_CN": "阻止血液从伤口流出。"
    },
    {
      "content": "彼女の傷は致命傷らしい。",
      "furigana": "[彼女|かの|じょ]の[傷|きず]は[致命傷|ち|めい|しょう]らしい。",
      "translation_zh_CN": "她的伤势看来是致命的。"
    },
    {
      "content": "そのリンゴは傷が付いている。",
      "furigana": "そのリンゴは[傷|きず]が[付|つ]いている。",
      "translation_zh_CN": "苹果被碰伤了。"
    },
    {
      "content": "ここにすり傷があります。",
      "furigana": "ここにすり[傷|きず]があります。",
      "translation_zh_CN": "这里有一个划痕。"
    },
    {
      "content": "誰も傷つけないで！",
      "furigana": "[誰|だれ]も[傷|きず]つけないで！",
      "translation_zh_CN": "别冒犯任何人。"
    },
    {
      "content": "傷付けないでくれ。",
      "furigana": "[傷付|きず|つ]けないでくれ。",
      "translation_zh_CN": "不要伤害我。"
    },
    {
      "content": "トムは傷を受けた。",
      "furigana": "トムは[傷|きず]を[受|う]けた。",
      "translation_zh_CN": "Tom负伤了。"
    },
    {
      "content": "牛乳が傷んでるよ。",
      "furigana": "[牛乳|ぎゅう|にゅう]が[傷|いた]んでるよ。",
      "translation_zh_CN": "牛奶变质了。"
    },
    {
      "content": "彼は戦争で負傷した。",
      "furigana": "[彼|かれ]は[戦争|せん|そう]で[負傷|ふ|しょう]した。",
      "translation_zh_CN": "他是在战争中受伤的。"
    },
    {
      "content": "彼は事故で負傷した。",
      "furigana": "[彼|かれ]は[事故|じ|こ]で[負傷|ふ|しょう]した。",
      "translation_zh_CN": "他在事故中受伤。"
    },
    {
      "content": "傷口からうみが出た。",
      "furigana": "[傷口|きず|ぐち]からうみが[出|で]た。",
      "translation_zh_CN": "伤口流出脓液。"
    },
    {
      "content": "傷あとが残りますか。",
      "furigana": "[傷|きず]あとが[残|のこ]りますか。",
      "translation_zh_CN": "我会留疤吗？"
    },
    {
      "content": "指先を火傷しました。",
      "furigana": "[指先|ゆび|さき]を[火傷|やけど]しました。",
      "translation_zh_CN": "我把我的指尖给烧伤了。"
    },
    {
      "content": "彼女を傷付けないで。",
      "furigana": "[彼女|かの|じょ]を[傷付|きず|つ]けないで。",
      "translation_zh_CN": "不要伤害她！"
    }
  ],
