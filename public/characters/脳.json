  "sentences": [
    {
      "content": "脳の構造は複雑だ。",
      "furigana": "[脳|のう]の[構造|こう|ぞう]は[複雑|ふく|ざつ]だ。",
      "translation_zh_CN": "大脑的结构很复杂。"
    },
    {
      "content": "脳は極めて複雑な臓器だ。",
      "furigana": "[脳|のう]は[極|きわ]めて[複雑|ふく|ざつ]な[臓器|ぞう|き]だ。",
      "translation_zh_CN": "大脑是一个极其复杂的器官。"
    },
    {
      "content": "私は脳挫傷を起こした。",
      "furigana": "[私|わたし]は[脳挫傷|のう|ざ|しょう]を[起|お]こした。",
      "translation_zh_CN": "我遭受了脑挫伤。"
    },
    {
      "content": "脳は肝臓や肺とは違っていた。",
      "furigana": "[脳|のう]は[肝臓|かん|ぞう]や[肺|はい]とは[違|ちが]っていた。",
      "translation_zh_CN": "大脑不同于肝脏和肺。"
    },
    {
      "content": "脳は頭の中にある。",
      "furigana": "[脳|のう]は[頭|あたま]の[中|なか]にある。",
      "translation_zh_CN": "大脑位于头部内部。"
    },
    {
      "content": "人間の目は脳と共に働く。",
      "furigana": "[人間|にん|げん]の[目|め]は[脳|のう]と[共|とも]に[働|はたら]く。",
      "translation_zh_CN": "人眼与大脑一起工作。"
    },
    {
      "content": "タバコは脳に影響を与える。",
      "furigana": "タバコは[脳|のう]に[影響|えい|きょう]を[与|あた]える。",
      "translation_zh_CN": "香烟会影响大脑。"
    },
    {
      "content": "その男は自分の脳を撃ち抜いた。",
      "furigana": "その[男|おとこ]は[自分|じ|ぶん]の[脳|のう]を[撃|う]ち[抜|ぬ]いた。",
      "translation_zh_CN": "该男子向自己的脑部开枪自杀。"
    },
    {
      "content": "彼の脳内で血管が破裂した。",
      "furigana": "[彼|かれ]の[脳内|のう|ない]で[血管|けっ|かん]が[破裂|は|れつ]した。",
      "translation_zh_CN": "他脑中的血管破裂了。"
    },
    {
      "content": "医者が僕の脳を調べたんだ。",
      "furigana": "[医者|い|しゃ]が[僕|ぼく]の[脳|のう]を[調|しら]べたんだ。",
      "translation_zh_CN": "医生检查了我的大脑。"
    },
    {
      "content": "脳の構造は複雑だ。",
      "furigana": "[脳|のう]の[構造|こう|ぞう]は[複雑|ふく|ざつ]だ。",
      "translation_zh_CN": "頭腦的結構很複雜。"
    },
    {
      "content": "トムは頭脳明晰です。",
      "furigana": "トムは[頭脳|ず|のう][明晰|めい|せき]です。",
      "translation_zh_CN": "汤姆脑子好使。"
    },
    {
      "content": "彼は優秀な脳外科医だ。",
      "furigana": "[彼|かれ]は[優秀|ゆう|しゅう]な[脳外科|のう|げ|か][医|い]だ。",
      "translation_zh_CN": "他是一個優秀的腦外科醫生。"
    },
    {
      "content": "去年脳卒中を起こしました。",
      "furigana": "[去年|きょ|ねん][脳卒中|のう|そっ|ちゅう]を[起|お]こしました。",
      "translation_zh_CN": "去年我得了中風。"
    },
    {
      "content": "小脳は血液の不断の供給を必要とする。",
      "furigana": "[小脳|しょう|のう]は[血液|けつ|えき]の[不断|ふ|だん]の[供給|きょう|きゅう]を[必要|ひつ|よう]とする。",
      "translation_zh_CN": "小脑需要连续不断的血液供给。"
    },
    {
      "content": "体が触れられたとき肌の感覚器官は脳に信号を送り、エンドルフィンのような化学物質が放出される。",
      "furigana": "[体|からだ]が[触|ふ]れられたとき[肌|はだ]の[感覚|かん|かく][器官|き|かん]は[脳|のう]に[信号|しん|ごう]を[送|おく]り、エンドルフィンのような[化学|か|がく][物質|ぶっ|しつ]が[放出|ほう|しゅつ]される。",
      "translation_zh_CN": "當身體受到外界觸碰的時候，皮膚的感受器就會向大腦發出信息，令大腦釋放安多芬等的化學物質。"
    }
  ],
