  "sentences": [
    {
      "content": "彼女はよく働く。",
      "furigana": "[彼女|かの|じょ]はよく[働|はたら]く。",
      "translation_zh_CN": "他努力工作。"
    },
    {
      "content": "働きすぎですよ。",
      "furigana": "[働|はたら]きすぎですよ。",
      "translation_zh_CN": "你工作太努力了。"
    },
    {
      "content": "一生懸命働いた。",
      "furigana": "[一生懸命|いっ|しょう|けん|めい][働|はたら]いた。",
      "translation_zh_CN": "他努力工作。"
    },
    {
      "content": "トムはよく働く。",
      "furigana": "トムはよく[働|はたら]く。",
      "translation_zh_CN": "汤姆是个好工人。"
    },
    {
      "content": "どこで働いてるの？",
      "furigana": "どこで[働|はたら]いてるの？",
      "translation_zh_CN": "请问你在哪里工作？"
    },
    {
      "content": "今働いています。",
      "furigana": "[今|いま][働|はたら]いています。",
      "translation_zh_CN": "我正在工作。"
    },
    {
      "content": "トムは働けない。",
      "furigana": "トムは[働|はたら]けない。",
      "translation_zh_CN": "汤姆不能工作了。"
    },
    {
      "content": "働き続けなさい。",
      "furigana": "[働|はたら]き[続|つづ]けなさい。",
      "translation_zh_CN": "继续工作！"
    },
    {
      "content": "昨夜、働いてたの？",
      "furigana": "[昨夜|ゆう|べ]、[働|はたら]いてたの？",
      "translation_zh_CN": "你昨晚在工作吗？"
    },
    {
      "content": "彼女は働き続けた。",
      "furigana": "[彼女|かの|じょ]は[働|はたら]き[続|つづ]けた。",
      "translation_zh_CN": "她继续工作。"
    }
