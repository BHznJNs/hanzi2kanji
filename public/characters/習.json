  "sentences": [
    {
      "content": "練習は熟達の道。",
      "furigana": "[練習|れん|しゅう]は[熟達|じゅく|たつ]の[道|みち]。",
      "translation_zh_CN": "熟能生巧。"
    },
    {
      "content": "習うより慣れろ。",
      "furigana": "[習|なら]うより[慣|な]れろ。",
      "translation_zh_CN": "熟能生巧。"
    },
    {
      "content": "英語を習いたいな。",
      "furigana": "[英語|えい|ご]を[習|なら]いたいな。",
      "translation_zh_CN": "我想学英语。"
    },
    {
      "content": "彼は泳ぎ方を習った。",
      "furigana": "[彼|かれ]は[泳|およ]ぎ[方|かた]を[習|なら]った。",
      "translation_zh_CN": "他学过游泳。"
    },
    {
      "content": "君は何を習ってるの。",
      "furigana": "[君|きみ]は[何|なに]を[習|なら]ってるの。",
      "translation_zh_CN": "你学什么？"
    },
    {
      "content": "トムを見習いなさい。",
      "furigana": "トムを[見習|み|なら]いなさい。",
      "translation_zh_CN": "好好向汤姆学学。"
    },
    {
      "content": "彼らは同じ習慣をもつ。",
      "furigana": "[彼|かれ]らは[同|おな]じ[習慣|しゅう|かん]をもつ。",
      "translation_zh_CN": "他们有相同的嗜好。"
    },
    {
      "content": "彼は毎日家で練習した。",
      "furigana": "[彼|かれ]は[毎日|まい|にち][家|か]で[練習|れん|しゅう]した。",
      "translation_zh_CN": "他每天在家练习。"
    },
    {
      "content": "彼は英語を習い始めた。",
      "furigana": "[彼|かれ]は[英語|えい|ご]を[習|なら]い[始|はじ]めた。",
      "translation_zh_CN": "他开始学英语了。"
    },
    {
      "content": "姉さんを見習いなさい。",
      "furigana": "[姉|ねえ]さんを[見習|み|なら]いなさい。",
      "translation_zh_CN": "学学你姐姐。"
    }
