  "sentences": [
    {
      "content": "名誉の証です。",
      "furigana": "[名誉|めい|よ]の[証|あかし]です。",
      "translation_zh_CN": "这是一种荣誉的标志。"
    },
    {
      "content": "搭乗券と荷物の引き換え証です。",
      "furigana": "[搭乗券|とう|じょう|けん]と[荷物|に|もつ]の[引|ひ]き[換|か]え[証|しょう]です。",
      "translation_zh_CN": "这是您的登机牌和行李兑换凭证。"
    },
    {
      "content": "車の免許証を失くしちゃった。",
      "furigana": "[車|くるま]の[免許証|めん|きょ|しょう]を[失|な]くしちゃった。",
      "translation_zh_CN": "我丢了汽车驾照。"
    },
    {
      "content": "君の免許証は期限が過ぎている。",
      "furigana": "[君|きみ]の[免許証|めん|きょ|しょう]は[期限|き|げん]が[過|す]ぎている。",
      "translation_zh_CN": "您的许可证已过期。"
    },
    {
      "content": "警官は私の免許証を調べた。",
      "furigana": "[警官|けい|かん]は[私|わたし]の[免許証|めん|きょ|しょう]を[調|しら]べた。",
      "translation_zh_CN": "警察检查了我的驾照。"
    },
    {
      "content": "学生３枚ください。これが学生証です。",
      "furigana": "[学生|がく|せい]３[枚|まい]ください。これが[学生証|がく|せい|しょう]です。",
      "translation_zh_CN": "请给我三张学生票。这是您的学生证。"
    },
    {
      "content": "運転免許証の有効期限が過ぎてますね。",
      "furigana": "[運転免許証|うん|てん|めん|きょ|しょう]の[有効期限|ゆう|こう|き|げん]が[過|す]ぎてますね。",
      "translation_zh_CN": "您的驾驶执照已过期。"
    },
    {
      "content": "毎月初めは保険証を提示してください。",
      "furigana": "[毎月初|まい|つき|はじ]めは[保険証|ほ|けん|しょう]を[提示|てい|じ]してください。",
      "translation_zh_CN": "请在每个月初出示您的保险卡。"
    },
    {
      "content": "悪質なドライバーは免許証を取り上げるべきだ。",
      "furigana": "[悪質|あく|しつ]なドライバーは[免許証|めん|きょ|しょう]を[取|と]り[上|あ]げるべきだ。",
      "translation_zh_CN": "不良司机的驾照应该被吊销。"
    },
    {
      "content": "私のコップにキスの証を残さないでください。",
      "furigana": "[私|わたし]のコップにキスの[証|あかし]を[残|のこ]さないでください。",
      "translation_zh_CN": "请不要在我的杯子上留下吻。"
    },
    {
      "content": "証拠は明らかだ。",
      "furigana": "[証拠|しょう|こ]は[明|あき]らかだ。",
      "translation_zh_CN": "证据确凿了。"
    },
    {
      "content": "学生証は持ってる？",
      "furigana": "[学生|がく|せい][証|しょう]は[持|も]ってる？",
      "translation_zh_CN": "你有學生證嗎？"
    },
    {
      "content": "保証は一年間です。",
      "furigana": "[保証|ほ|しょう]は[一|いち][年間|ねん|かん]です。",
      "translation_zh_CN": "我們保證我們的產品一年。"
    },
    {
      "content": "それ、証明できるの？",
      "furigana": "それ、[証明|しょう|めい]できるの？",
      "translation_zh_CN": "你能证明吗?"
    },
    {
      "content": "それを証明できますか？",
      "furigana": "それを[証明|しょう|めい]できますか？",
      "translation_zh_CN": "你能证明吗?"
    },
    {
      "content": "学生証は、ありますか？",
      "furigana": "[学生|がく|せい][証|しょう]は、ありますか？",
      "translation_zh_CN": "你有学生证吗？"
    },
    {
      "content": "免許証はお持ちですか。",
      "furigana": "[免許|めん|きょ][証|しょう]はお[持|も]ちですか。",
      "translation_zh_CN": "你有駕駛執照嗎？"
    },
    {
      "content": "何も保証はできないよ。",
      "furigana": "[何|なに]も[保証|ほ|しょう]はできないよ。",
      "translation_zh_CN": "我们什么都不能承诺。"
    },
    {
      "content": "免許証を見せてください。",
      "furigana": "[免許|めん|きょ][証|しょう]を[見|み]せてください。",
      "translation_zh_CN": "請給我看看你的駕駛執照。"
    },
    {
      "content": "運転免許証を持っていますか。",
      "furigana": "[運転|うん|てん][免許|めん|きょ][証|しょう]を[持|も]っていますか。",
      "translation_zh_CN": "您有駕照嗎？"
    }
  ],
