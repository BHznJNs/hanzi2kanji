    },
    {
      "content": "鳥は歌います。",
      "furigana": "[鳥|とり]は[歌|うた]います。",
      "translation_zh_CN": "鸟儿歌唱。"
    },
    {
      "content": "鳥が飛んでる。",
      "furigana": "[鳥|とり]が[飛|と]んでる。",
      "translation_zh_CN": "鸟儿在飞翔。"
    },
    {
      "content": "鳥が歌っている。",
      "furigana": "[鳥|とり]が[歌|うた]っている。",
      "translation_zh_CN": "鸟儿在歌唱"
    },
    {
      "content": "鳥が描けない。",
      "furigana": "[鳥|とり]が[描|か]けない。",
      "translation_zh_CN": "我不会画鸟。"
    },
    {
      "content": "鳥が囀る。",
      "furigana": "[鳥|とり]が[囀|さえず]る。",
      "translation_zh_CN": "鸟儿歌唱。"
    },
    {
      "content": "鳥になりたいな。",
      "furigana": "[鳥|とり]になりたいな。",
      "translation_zh_CN": "我想成为一只鸟。"
    },
    {
      "content": "鳥になれたらなあ。",
      "furigana": "[鳥|とり]になれたらなあ。",
      "translation_zh_CN": "我希望我能成为一只鸟。"
    },
    {
      "content": "鳥は高く舞い上がった。",
      "furigana": "[鳥|とり]は[高|たか]く[舞|ま]い[上|あ]がった。",
      "translation_zh_CN": "那只鸟飞得很高。"
    },
    {
      "content": "鳥には翼がある。",
      "furigana": "[鳥|とり]には[翼|つばさ]がある。",
      "translation_zh_CN": "鸟有翅膀。"
    },
    {
      "content": "一石二鳥にする。",
      "furigana": "[一石二鳥|いっ|せき|に|ちょう]にする。",
      "translation_zh_CN": "一石二鸟。"
    },
    {
      "content": "鳥は巣をつくる。",
      "furigana": "[鳥|とり]は[巣|す]をつくる。",
      "translation_zh_CN": "鸟儿筑巢。"
    },
    {
      "content": "鳥が空にいます。",
      "furigana": "[鳥|とり]が[空|そら]にいます。",
      "translation_zh_CN": "鸟在天上。"
    },
    {
      "content": "鳥は巣作りをする。",
      "furigana": "[鳥|とり]は[巣|す][作|づく]りをする。",
      "translation_zh_CN": "鸟儿筑巢。"
    },
    {
      "content": "この鳥は何て言うの？",
      "furigana": "この[鳥|とり]は[何|なん]て[言|い]うの？",
      "translation_zh_CN": "这只鸟叫作什么?"
    },
    {
      "content": "この鳥は飛べません。",
      "furigana": "この[鳥|とり]は[飛|と]べません。",
      "translation_zh_CN": "这只鸟不能飞。"
    },
    {
      "content": "森では鳥が歌っていた。",
      "furigana": "[森|もり]では[鳥|とり]が[歌|うた]っていた。",
      "translation_zh_CN": "鸟儿在森林里歌唱。"
    },
    {
      "content": "この七面鳥おいしいね。",
      "furigana": "この[七面鳥|しち|めん|ちょう]おいしいね。",
      "translation_zh_CN": "这只火鸡味道很好。"
    },
    {
      "content": "ガチョウは水鳥である。",
