  "sentences": [
    {
      "content": "陸が見えてきた。",
      "furigana": "[陸|りく]が[見|み]えてきた。",
      "translation_zh_CN": "陆地映入眼帘。"
    },
    {
      "content": "遠くに陸が見えた。",
      "furigana": "[遠|とお]くに[陸|りく]が[見|み]えた。",
      "translation_zh_CN": "我可以看到远处的陆地。"
    },
    {
      "content": "水夫たちは陸を見た。",
      "furigana": "[水夫|すい|ふ]たちは[陸|りく]を[見|み]た。",
      "translation_zh_CN": "水手们看着陆地。"
    },
    {
      "content": "たった今漁船が陸を離れた。",
      "furigana": "たった[今漁船|いま|ぎょ|せん]が[陸|りく]を[離|はな]れた。",
      "translation_zh_CN": "一艘渔船刚刚离开岸边。"
    },
    {
      "content": "地球は海と陸からなる。",
      "furigana": "[地球|ち|きゅう]は[海|うみ]と[陸|りく]からなる。",
      "translation_zh_CN": "地球由海洋和陆地组成。"
    },
    {
      "content": "船はしだいに陸に近づいた。",
      "furigana": "[船|ふね]はしだいに[陸|りく]に[近|ちか]づいた。",
      "translation_zh_CN": "船逐渐靠近陆地。"
    },
    {
      "content": "強風が陸の方へ吹き始めた。",
      "furigana": "[強風|きょう|ふう]が[陸|りく]の[方|ほう]へ[吹|ふ]き[始|はじ]めた。",
      "translation_zh_CN": "一阵强风开始向陆地吹来。"
    },
    {
      "content": "クジラは昔陸に住んでいたと言われている。",
      "furigana": "クジラは[昔陸|むかし|りく]に[住|す]んでいたと[言|い]われている。",
      "translation_zh_CN": "据说鲸鱼曾经生活在陆地上。"
    },
    {
      "content": "陸と水で地球の表面は出来ている。",
      "furigana": "[陸|りく]と[水|みず]で[地球|ち|きゅう]の[表面|ひょう|めん]は[出来|で|き]ている。",
      "translation_zh_CN": "地球表面由陆地和水组成。"
    },
    {
      "content": "君はいわば陸に上がった魚のようなものだ。",
      "furigana": "[君|きみ]はいわば[陸|りく]に[上|あ]がった[魚|さかな]のようなものだ。",
      "translation_zh_CN": "你就像陆地上的鱼。"
    },
    {
      "content": "陸に上がった河童。",
      "furigana": "[陸|おか]に[上|あ]がった[河童|かっぱ]。",
      "translation_zh_CN": "釜底游鱼。"
    },
    {
      "content": "彼は陸軍に入った。",
      "furigana": "[彼|かれ]は[陸軍|りく|ぐん]に[入|はい]った。",
      "translation_zh_CN": "他加入了陆军。"
    },
    {
      "content": "飛行機は離陸間際だ。",
      "furigana": "[飛行機|ひ|こう|き]は[離陸|り|りく][間際|ま|ぎわ]だ。",
      "translation_zh_CN": "飞机快要起飞了。"
    },
    {
      "content": "飛行機は無事着陸した。",
      "furigana": "[飛行機|ひ|こう|き]は[無事|ぶ|じ][着陸|ちゃく|りく]した。",
      "translation_zh_CN": "这架飞机安全著陆了。"
    },
    {
      "content": "明日、彼は月に着陸する。",
      "furigana": "[明日|あした]、[彼|かれ]は[月|つき]に[着陸|ちゃく|りく]する。",
      "translation_zh_CN": "明天，他将登上月球。"
    },
    {
      "content": "台風が関東地方に上陸した。",
      "furigana": "[台風|たい|ふう]が[関東|かん|とう][地方|ち|ほう]に[上陸|じょう|りく]した。",
      "translation_zh_CN": "台风袭击了关东地区。"
    },
    {
      "content": "当機は安全に着陸しました。",
      "furigana": "[当機|とう|き]は[安全|あん|ぜん]に[着陸|ちゃく|りく]しました。",
      "translation_zh_CN": "这架飞机安全著陆了。"
    },
    {
      "content": "飛行機は十分後に離陸します。",
      "furigana": "[飛行機|ひ|こう|き]は[十|じゅっ][分|ぷん][後|ご]に[離陸|り|りく]します。",
      "translation_zh_CN": "飞机十分钟后起飞。"
    },
    {
      "content": "飛行機は１０分後に離陸します。",
      "furigana": "[飛行機|ひ|こう|き]は[１０|じゅっ][分|ぷん][後|ご]に[離陸|り|りく]します。",
      "translation_zh_CN": "飞机十分钟后起飞。"
    },
    {
      "content": "ジェット機は東京に着陸した。",
      "furigana": "ジェット[機|き]は[東京|とう|きょう]に[着陸|ちゃく|りく]した。",
      "translation_zh_CN": "这架客机降落在东京了。"
    }
  ],
