  "sentences": [
    {
      "content": "彼は営業部の部長です。",
      "furigana": "[彼|かれ]は[営業|えい|ぎょう][部|ぶ]の[部長|ぶ|ちょう]です。",
      "translation_zh_CN": "他是营销部的负责人。"
    },
    {
      "content": "日曜日は営業してますか？",
      "furigana": "[日曜日|にち|よう|び]は[営業|えい|ぎょう]してますか？",
      "translation_zh_CN": "星期日营业吗？"
    },
    {
      "content": "営業時間を教えてください。",
      "furigana": "[営業|えい|ぎょう][時間|じ|かん]を[教|おし]えてください。",
      "translation_zh_CN": "请告诉我营业时间。"
    },
    {
      "content": "おじはホテルを経営している。",
      "furigana": "おじはホテルを[経営|けい|えい]している。",
      "translation_zh_CN": "我叔叔开旅馆。"
    },
    {
      "content": "父はレストランを経営している。",
      "furigana": "[父|ちち]はレストランを[経営|けい|えい]している。",
      "translation_zh_CN": "我的父亲经营一家餐馆。"
    },
    {
      "content": "彼は町でスーパーを経営している。",
      "furigana": "[彼|かれ]は[町|まち]でスーパーを[経営|けい|えい]している。",
      "translation_zh_CN": "他在这个镇上经营一家超级市场。"
    },
    {
      "content": "その会社は外国人が経営している。",
      "furigana": "その[会社|かい|しゃ]は[外国|がい|こく][人|じん]が[経営|けい|えい]している。",
      "translation_zh_CN": "这家公司由一个外国人经营。"
    },
    {
      "content": "両陣営ともに数千人単位の負傷者が出た。",
      "furigana": "[両|りょう][陣営|じん|えい]ともに[数|すう][千|せん][人|にん][単位|たん|い]の[負傷|ふ|しょう][者|しゃ]が[出|で]た。",
      "translation_zh_CN": "双方都有几千人受伤。"
    },
    {
      "content": "お父さんはレストランを経営しているのよ。",
      "furigana": "お[父|とう]さんはレストランを[経営|けい|えい]しているのよ。",
      "translation_zh_CN": "我的父亲经营一家餐馆。"
    },
    {
      "content": "そのラーメン屋は夜中でも営業をしている。",
      "furigana": "そのラーメン[屋|や]は[夜中|よ|なか]でも[営業|えい|ぎょう]をしている。",
      "translation_zh_CN": "那间拉面店在半夜也营业"
    }
