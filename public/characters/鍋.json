  "sentences": [
    {
      "content": "鍋に油入れた？",
      "furigana": "[鍋|なべ]に[油入|あぶら|い]れた？",
      "translation_zh_CN": "锅里放了油吗？"
    },
    {
      "content": "鍋を焦げつかせるな。",
      "furigana": "[鍋|なべ]を[焦|こ]げつかせるな。",
      "translation_zh_CN": "别让锅烧着了。"
    },
    {
      "content": "金は鍋を煮え立たせる。",
      "furigana": "[金|きむ]は[鍋|なべ]を[煮|に]え[立|た]たせる。",
      "translation_zh_CN": "有钱能使锅沸腾。"
    },
    {
      "content": "鍋がふきこぼれているよ。",
      "furigana": "[鍋|なべ]がふきこぼれているよ。",
      "translation_zh_CN": "锅已经沸腾了。"
    },
    {
      "content": "鍋に蓋をして。",
      "furigana": "[鍋|なべ]に[蓋|ぶた]をして。",
      "translation_zh_CN": "盖上锅盖。"
    },
    {
      "content": "大きい鍋で汁を作った。",
      "furigana": "[大|おお]きい[鍋|なべ]で[汁|しる]を[作|つく]った。",
      "translation_zh_CN": "我在一个大锅里煮汤。"
    },
    {
      "content": "好きな勉強だけしていても実力はつきませんよ。",
      "furigana": "[好|す]きな[勉強|べん|きょう]だけしていても[実力|じつ|りょく]はつきませんよ。",
      "translation_zh_CN": "如果你只学习你喜欢的东西，你不会提高你的技能。"
    },
    {
      "content": "大きい鍋でスープを作った。",
      "furigana": "[大|おお]きい[鍋|なべ]でスープを[作|つく]った。",
      "translation_zh_CN": "我在一个大锅里煮汤。"
    },
    {
      "content": "小鍋はすぐ熱くなる。",
      "furigana": "[小鍋|しょう|なべ]はすぐ[熱|あつ]くなる。",
      "translation_zh_CN": "小锅很快就热起来了。"
    },
    {
      "content": "平なべを火からおろしなさい。",
      "furigana": "[平|ひら]なべを[火|ひ]からおろしなさい。",
      "translation_zh_CN": "将锅从火上移开。"
    },
    {
      "content": "大きい鍋で汁を作った。",
      "furigana": "[大|おお]きい[鍋|なべ]で[汁|しる]を[作|つく]った。",
      "translation_zh_CN": "用大锅煲汤。"
    }
