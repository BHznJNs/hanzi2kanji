  "sentences": [
    {
      "content": "彼は読書に飽きた。",
      "furigana": "[彼|かれ]は[読書|どく|しょ]に[飽|あ]きた。",
      "translation_zh_CN": "他厌倦了读书。"
    },
    {
      "content": "私は書くことに飽きた。",
      "furigana": "[私|わたし]は[書|か]くことに[飽|あ]きた。",
      "translation_zh_CN": "我写腻了。"
    },
    {
      "content": "もうこの番組には飽きた。",
      "furigana": "もうこの[番組|ばん|ぐみ]には[飽|あ]きた。",
      "translation_zh_CN": "我已经受够了这个计划。"
    },
    {
      "content": "彼は本を読むのに飽きた。",
      "furigana": "[彼|かれ]は[本|ほん]を[読|よ]むのに[飽|あ]きた。",
      "translation_zh_CN": "他厌倦了读书。"
    },
    {
      "content": "ファストフードはもう飽きた。",
      "furigana": "ファストフードはもう[飽|あ]きた。",
      "translation_zh_CN": "我厌倦吃快餐了。"
    },
    {
      "content": "刺身もこう毎日だと飽きるな。",
      "furigana": "[刺身|さし|み]もこう[毎日|まい|にち]だと[飽|あ]きるな。",
      "translation_zh_CN": "每天都这样地吃生鱼片真是太腻歪了。"
    },
    {
      "content": "僕はテレビを見るのに飽きたよ。",
      "furigana": "[僕|ぼく]はテレビを[見|み]るのに[飽|あ]きたよ。",
      "translation_zh_CN": "我厌倦了看电视。"
    },
    {
      "content": "よくも飽きずに毎日同じ事ができるね。",
      "furigana": "よくも[飽|あ]きずに[毎日|まい|にち][同|おな]じ[事|じ]ができるね。",
      "translation_zh_CN": "你每天都是做一模一样的东西，难道不会觉得闷吗？"
    },
    {
      "content": "ファーストフードには飽き飽きしてるんだ。",
      "furigana": "ファーストフードには[飽|あ]き[飽|あ]きしてるんだ。",
      "translation_zh_CN": "我受够吃快餐了。"
    },
    {
      "content": "僕はレストランの食事には飽き飽きしている。",
      "furigana": "[僕|ぼく]はレストランの[食事|しょく|じ]には[飽|あ]き[飽|あ]きしている。",
      "translation_zh_CN": "我受够了在餐馆吃饭。"
    }
  ],
