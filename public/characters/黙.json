  "sentences": [
    {
      "content": "黙っていなさい。",
      "furigana": "[黙|だま]っていなさい。",
      "translation_zh_CN": "別作聲！"
    },
    {
      "content": "だから黙れってば！",
      "furigana": "だから[黙|だま]れってば！",
      "translation_zh_CN": "我说过了，闭嘴！"
    },
    {
      "content": "沈黙は同意を表す。",
      "furigana": "[沈黙|ちん|もく]は[同意|どう|い]を[表|あらわ]す。",
      "translation_zh_CN": "沉默就意味着同意。"
    },
    {
      "content": "黙れっつってんだろ！",
      "furigana": "[黙|だま]れっつってんだろ！",
      "translation_zh_CN": "我说过了，闭嘴！"
    },
    {
      "content": "雄弁は銀、沈黙は金。",
      "furigana": "[雄弁|ゆう|べん]は[銀|ぎん]、[沈黙|ちん|もく]は[金|きん]。",
      "translation_zh_CN": "开口是银，沉默是金。"
    },
    {
      "content": "その子は黙っていた。",
      "furigana": "その[子|こ]は[黙|だま]っていた。",
      "translation_zh_CN": "男孩依旧沉默不语。"
    },
    {
      "content": "彼は一日中黙っていた。",
      "furigana": "[彼|かれ]は[一|いち][日|にち][中|じゅう][黙|だま]っていた。",
      "translation_zh_CN": "他一整天都沉默着。"
    },
    {
      "content": "ちょっと黙ってくれんか。",
      "furigana": "ちょっと[黙|だま]ってくれんか。",
      "translation_zh_CN": "为什么不让你的嘴巴休息呢？"
    },
    {
      "content": "ちょっと黙っててくれない？",
      "furigana": "ちょっと[黙|だま]っててくれない？",
      "translation_zh_CN": "为什么不让你的嘴巴休息呢？"
    },
    {
      "content": "「黙って」と彼はささやいた。",
      "furigana": "「[黙|だま]って」と[彼|かれ]はささやいた。",
      "translation_zh_CN": "“闭嘴。”他嗫嚅道。"
    }
  ],
