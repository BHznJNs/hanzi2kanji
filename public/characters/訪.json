  "sentences": [
    {
      "content": "明日彼を訪問します。",
      "furigana": "[明日|あす][彼|かれ]を[訪問|ほう|もん]します。",
      "translation_zh_CN": "明天我將拜訪他。"
    },
    {
      "content": "わたしは韓国を訪れたい。",
      "furigana": "わたしは[韓国|かん|こく]を[訪|おとず]れたい。",
      "translation_zh_CN": "我想访问韩国。"
    },
    {
      "content": "彼は三回フランスを訪れた。",
      "furigana": "[彼|かれ]は[三|さん][回|かい]フランスを[訪|おとず]れた。",
      "translation_zh_CN": "他訪問過法國三次。"
    },
    {
      "content": "この美術館は訪問に値する。",
      "furigana": "この[美術館|び|じゅつ|かん]は[訪問|ほう|もん]に[値|あたい]する。",
      "translation_zh_CN": "那个博物馆值得参观。"
    },
    {
      "content": "彼女はよくこの国を訪れます。",
      "furigana": "[彼女|かの|じょ]はよくこの[国|くに]を[訪|おとず]れます。",
      "translation_zh_CN": "她经常来这个国家。"
    },
    {
      "content": "彼女の夢はパリを訪れることだ。",
      "furigana": "[彼女|かの|じょ]の[夢|ゆめ]はパリを[訪|おとず]れることだ。",
      "translation_zh_CN": "她的梦想是参观巴黎。"
    },
    {
      "content": "なぜ、英国を訪問するのですか。",
      "furigana": "なぜ、[英国|えい|こく]を[訪問|ほう|もん]するのですか。",
      "translation_zh_CN": "你為何走訪英國？"
    },
    {
      "content": "友達が私の家に訪ねてきました。",
      "furigana": "[友達|とも|だち]が[私|わたし]の[家|いえ]に[訪|たず]ねてきました。",
      "translation_zh_CN": "朋友来我家拜访了。"
    },
    {
      "content": "私が訪問した時彼女は留守だった。",
      "furigana": "[私|わたし]が[訪問|ほう|もん]した[時|とき][彼女|かの|じょ]は[留守|る|す]だった。",
      "translation_zh_CN": "我给她打电话的时候她不在。"
    },
    {
      "content": "僕はおばあちゃんの家を訪ねたんだ。",
      "furigana": "[僕|ぼく]はおばあちゃんの[家|いえ]を[訪|たず]ねたんだ。",
      "translation_zh_CN": "我拜訪了我祖母的房子。"
    }
  ],
