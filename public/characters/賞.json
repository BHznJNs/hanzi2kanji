  "sentences": [
    {
      "content": "賞は彼の手に。",
      "furigana": "[賞|しょう]は[彼|かれ]の[手|て]に。",
      "translation_zh_CN": "奖品就在他手里。"
    },
    {
      "content": "ビルが特別賞に選ばれた。",
      "furigana": "ビルが[特別賞|とく|べつ|しょう]に[選|えら]ばれた。",
      "translation_zh_CN": "比尔被选为一项特别奖。"
    },
    {
      "content": "彼はその賞に値する。",
      "furigana": "[彼|かれ]はその[賞|しょう]に[値|あたい]する。",
      "translation_zh_CN": "他值得这个奖项。"
    },
    {
      "content": "私は優等賞を受けた。",
      "furigana": "[私|わたし]は[優等賞|ゆう|とう|しょう]を[受|う]けた。",
      "translation_zh_CN": "我获得了荣誉奖。"
    },
    {
      "content": "彼らは賞を目指して争っている。",
      "furigana": "[彼|かれ]らは[賞|しょう]を[目指|め|ざ]して[争|あらそ]っている。",
      "translation_zh_CN": "他们正在争夺奖项。"
    },
    {
      "content": "彼は１等賞を与えられた。",
      "furigana": "[彼|かれ]は１[等賞|とう|しょう]を[与|あた]えられた。",
      "translation_zh_CN": "他被授予一等奖。"
    },
    {
      "content": "彼女が賞を取りそうだ。",
      "furigana": "[彼女|かの|じょ]が[賞|しょう]を[取|と]りそうだ。",
      "translation_zh_CN": "我认为她会获奖。"
    },
    {
      "content": "彼女はその賞をもらいました。",
      "furigana": "[彼女|かの|じょ]はその[賞|しょう]をもらいました。",
      "translation_zh_CN": "她获奖了。"
    },
    {
      "content": "彼女がその賞を受賞しました。",
      "furigana": "[彼女|かの|じょ]がその[賞|しょう]を[受賞|じゅ|しょう]しました。",
      "translation_zh_CN": "她获奖了。"
    },
    {
      "content": "マザー・テレサはノーベル賞を受賞した。",
      "furigana": "マザー・テレサはノーベル[賞|しょう]を[受賞|じゅ|しょう]した。",
      "translation_zh_CN": "特蕾莎修女荣获诺贝尔奖。"
    },
    {
      "content": "１等賞を取ったぞ。",
      "furigana": "[１等|いっとう][賞|しょう]を[取|と]ったぞ。",
      "translation_zh_CN": "我贏得了第一名！"
    },
    {
      "content": "彼は三位に入賞した。",
      "furigana": "[彼|かれ]は[三|さん][位|い]に[入賞|にゅう|しょう]した。",
      "translation_zh_CN": "他贏了三等奬。"
    },
    {
      "content": "このパンって賞味期限大丈夫か？",
      "furigana": "このパンって[賞味|しょう|み][期限|き|げん][大丈夫|だい|じょう|ぶ]か？",
      "translation_zh_CN": "这个面包的保质期可以吗？"
    },
    {
      "content": "一等賞は王女からのキスです。",
      "furigana": "[一等|いっ|とう][賞|しょう]は[王女|おう|じょ]からのキスです。",
      "translation_zh_CN": "一等奖是公主的香吻。"
    },
    {
      "content": "１０チームが賞を得ようと争った。",
      "furigana": "[１|][０|]チームが[賞|しょう]を[得|え]ようと[争|あらそ]った。",
      "translation_zh_CN": "十支隊伍競逐獎項。"
    },
    {
      "content": "トムには近代絵画の鑑賞能力がある。",
      "furigana": "トムには[近代|きん|だい][絵画|かい|が]の[鑑賞|かん|しょう][能力|のう|りょく]がある。",
      "translation_zh_CN": "汤姆欣赏现代艺术。"
    },
    {
      "content": "１等賞を得たのはジャネットだった。",
      "furigana": "[１|][等|とう][賞|しょう]を[得|え]たのはジャネットだった。",
      "translation_zh_CN": "是珍妮贏得了第一名。"
    },
    {
      "content": "トムの望みは一等賞を取ることだった。",
      "furigana": "トムの[望|のぞ]みは[一等|いっ|とう][賞|しょう]を[取|と]ることだった。",
      "translation_zh_CN": "湯姆希望贏得第一名。"
    },
    {
      "content": "あの映画を鑑賞できるのは大人だけだ。",
      "furigana": "あの[映画|えい|が]を[鑑賞|かん|しょう]できるのは[大人|おとな]だけだ。",
      "translation_zh_CN": "只有成人可以看那种电影。"
    },
    {
      "content": "マザー・テレサはノーベル賞を受賞した。",
      "furigana": "マザー・テレサはノーベル[賞|しょう]を[受賞|じゅ|しょう]した。",
      "translation_zh_CN": "特蕾莎修女被授予諾貝爾獎。"
    }
  ],
