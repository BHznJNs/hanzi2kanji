  "sentences": [
    {
      "content": "彼は評判がよい。",
      "furigana": "[彼|かれ]は[評判|ひょう|ばん]がよい。",
      "translation_zh_CN": "他的名声很好。"
    },
    {
      "content": "トムは評判が悪い。",
      "furigana": "トムは[評判|ひょう|ばん]が[悪|わる]い。",
      "translation_zh_CN": "Tom的名声不好。"
    },
    {
      "content": "私は彼女を高く評価した。",
      "furigana": "[私|わたし]は[彼女|かの|じょ]を[高|たか]く[評価|ひょう|か]した。",
      "translation_zh_CN": "我對她的評價很好。"
    },
    {
      "content": "音楽の重要性は過小評価されている。",
      "furigana": "[音楽|おん|がく]の[重要|じゅう|よう][性|せい]は[過小|か|しょう][評価|ひょう|か]されている。",
      "translation_zh_CN": "音乐的重要性被低估了。"
    },
    {
      "content": "彼はひっきりなしに他人を批評する。",
      "furigana": "[彼|かれ]はひっきりなしに[他人|た|にん]を[批評|ひ|ひょう]する。",
      "translation_zh_CN": "他三番五次评判别人。"
    },
    {
      "content": "自分の力を過小評価しちゃだめだよ。",
      "furigana": "[自分|じ|ぶん]の[力|ちから]を[過小|か|しょう][評価|ひょう|か]しちゃだめだよ。",
      "translation_zh_CN": "不要低估自己的实力。"
    },
    {
      "content": "公平に評すれば、彼は最善を尽くした。",
      "furigana": "[公平|こう|へい]に[評|ひょう]すれば、[彼|かれ]は[最善|さい|ぜん]を[尽|つ]くした。",
      "translation_zh_CN": "说句实话，他真的尽力了。"
    },
    {
      "content": "その本についての彼の論評は好意的だった。",
      "furigana": "その[本|ほん]についての[彼|かれ]の[論評|ろん|ぴょう]は[好意|こう|い][的|てき]だった。",
      "translation_zh_CN": "他給這本書正面的評價。"
    },
    {
      "content": "この評論を優勝作品に選んだ基準は何ですか。",
      "furigana": "この[評論|ひょう|ろん]を[優勝|ゆう|しょう][作品|さく|ひん]に[選|えら]んだ[基準|き|じゅん]は[何|なん]ですか。",
      "translation_zh_CN": "您是用了什么标准选定这篇评论胜出呢？"
    },
    {
      "content": "彼らは史上最高のロックバンドだと評価されている。",
      "furigana": "[彼|かれ]らは[史上|し|じょう][最高|さい|こう]のロックバンドだと[評価|ひょう|か]されている。",
      "translation_zh_CN": "有人认为他们是历史上最伟大的乐队。"
    }
