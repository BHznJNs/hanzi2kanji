  "sentences": [
    {
      "content": "幸雄の顔で ",
      "furigana": "[幸雄|ゆきお]の[顔|かお]で ",
      "translation_zh_CN": "有了由纪夫的脸"
    },
    {
      "content": "顔を洗いなさい。",
      "furigana": "[顔|かお]を[洗|あら]いなさい。",
      "translation_zh_CN": "洗脸。"
    },
    {
      "content": "顔赤いよ。",
      "furigana": "[顔赤|かお|あか]いよ。",
      "translation_zh_CN": "你的脸红了。"
    },
    {
      "content": "顔がひきつります。",
      "furigana": "[顔|かお]がひきつります。",
      "translation_zh_CN": "我的脸抽搐。"
    },
    {
      "content": "顔が青いよ。",
      "furigana": "[顔|かお]が[青|あお]いよ。",
      "translation_zh_CN": "你的脸是蓝色的。"
    },
    {
      "content": "二度と顔を出すな。",
      "furigana": "[二度|に|ど]と[顔|かお]を[出|だ]すな。",
      "translation_zh_CN": "别再露脸了。"
    },
    {
      "content": "顔をそってください。",
      "furigana": "[顔|かお]をそってください。",
      "translation_zh_CN": "请刮脸。"
    },
    {
      "content": "トムの顔が赤い。",
      "furigana": "トムの[顔|かお]が[赤|あか]い。",
      "translation_zh_CN": "汤姆的脸变红了。"
    },
    {
      "content": "顔を奇麗にふきなさい。",
      "furigana": "[顔|かお]を[奇麗|き|れい]にふきなさい。",
      "translation_zh_CN": "把脸擦干净。"
    },
    {
      "content": "トムの顔が見えなかった。",
      "furigana": "トムの[顔|かお]が[見|み]えなかった。",
      "translation_zh_CN": "我看不到汤姆的脸。"
    },
    {
      "content": "彼女は丸顔です。",
      "furigana": "[彼女|かの|じょ]は[丸|まる][顔|かお]です。",
      "translation_zh_CN": "她有张圆脸。"
    },
    {
      "content": "顔を洗いなさい。",
      "furigana": "[顔|かお]を[洗|あら]いなさい。",
      "translation_zh_CN": "洗脸吧。"
    },
    {
      "content": "トムの顔が赤い。",
      "furigana": "トムの[顔|かお]が[赤|あか]い。",
      "translation_zh_CN": "汤姆的脸红了。"
    },
    {
      "content": "いつも笑顔でね。",
      "furigana": "いつも[笑顔|え|がお]でね。",
      "translation_zh_CN": "保持微笑。"
    },
    {
      "content": "トムは顔が赤い。",
      "furigana": "トムは[顔|かお]が[赤|あか]い。",
      "translation_zh_CN": "汤姆的脸红了。"
    },
    {
      "content": "トムは顔が広い。",
      "furigana": "トムは[顔|かお]が[広|ひろ]い。",
      "translation_zh_CN": "汤姆熟人多。"
    },
    {
      "content": "二度と顔を出すな。",
      "furigana": "[二度|に|ど]と[顔|かお]を[出|だ]すな。",
      "translation_zh_CN": "我不想再見到你！"
    },
    {
      "content": "窓から顔を出すな。",
      "furigana": "[窓|まど]から[顔|かお]を[出|だ]すな。",
      "translation_zh_CN": "不要把脸探向窗外。"
    },
    {
      "content": "顔色が悪いですよ。",
      "furigana": "[顔色|かお|いろ]が[悪|わる]いですよ。",
      "translation_zh_CN": "你的脸色不好。"
    },
    {
      "content": "トムは顔色が悪い。",
      "furigana": "トムは[顔色|かお|いろ]が[悪|わる]い。",
      "translation_zh_CN": "湯姆看起來很蒼白。"
    }
  ],
