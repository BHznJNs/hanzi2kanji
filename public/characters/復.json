  "sentences": [
    {
      "content": "往復旅行？片道だけです。",
      "furigana": "[往復|おう|ふく][旅行|りょ|こう]？[片道|かた|みち]だけです。",
      "translation_zh_CN": "往返吗？是单程。"
    },
    {
      "content": "往復切符を買いました。",
      "furigana": "[往復|おう|ふく][切符|きっ|ぷ]を[買|か]いました。",
      "translation_zh_CN": "我买了往返的票。"
    },
    {
      "content": "往復びんたを食らった。",
      "furigana": "[往復|おう|ふく]びんたを[食|く]らった。",
      "translation_zh_CN": "我两个脸颊都被打了。"
    },
    {
      "content": "彼は回復の見込みが無い。",
      "furigana": "[彼|かれ]は[回復|かい|ふく]の[見込|み|こ]みが[無|な]い。",
      "translation_zh_CN": "对于他的康复没有任何希望。"
    },
    {
      "content": "往復切符を買いましたか。",
      "furigana": "[往復|おう|ふく][切符|きっ|ぷ]を[買|か]いましたか。",
      "translation_zh_CN": "你买了往返的票吗？"
    },
    {
      "content": "往復ですか、片道ですか。",
      "furigana": "[往復|おう|ふく]ですか、[片道|かた|みち]ですか。",
      "translation_zh_CN": "往返还是单程？"
    },
    {
      "content": "トムは報復を望んでいる。",
      "furigana": "トムは[報復|ほう|ふく]を[望|のぞ]んでいる。",
      "translation_zh_CN": "汤姆想要复仇。"
    },
    {
      "content": "彼は回復する見込みがない。",
      "furigana": "[彼|かれ]は[回復|かい|ふく]する[見込|み|こ]みがない。",
      "translation_zh_CN": "对于他的康复没有任何希望。"
    },
    {
      "content": "彼が回復する見込みは全くない。",
      "furigana": "[彼|かれ]が[回復|かい|ふく]する[見込|み|こ]みは[全|まった]くない。",
      "translation_zh_CN": "他沒有希望痊癒了。"
    },
    {
      "content": "復活祭、おめでとうございます。",
      "furigana": "[復活|ふっ|かつ][祭|さい]、おめでとうございます。",
      "translation_zh_CN": "复活节快乐。"
    }
  ],
