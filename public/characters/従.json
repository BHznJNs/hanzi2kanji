  "sentences": [
    {
      "content": "両親には従うべきだ。",
      "furigana": "[両親|りょう|しん]には[従|したが]うべきだ。",
      "translation_zh_CN": "你该听你父母的话。"
    },
    {
      "content": "従兄弟が３人います。",
      "furigana": "[従兄弟|いとこ]が[３人|さんにん]います。",
      "translation_zh_CN": "我有三个堂兄。"
    },
    {
      "content": "君は両親に従うべきだ。",
      "furigana": "[君|きみ]は[両親|りょう|しん]に[従|したが]うべきだ。",
      "translation_zh_CN": "你该听你父母的话。"
    },
    {
      "content": "郷に入っては郷に従え。",
      "furigana": "[郷|ごう]に[入|い]っては[郷|ごう]に[従|したが]え。",
      "translation_zh_CN": "入乡随俗。"
    },
    {
      "content": "彼らは私の意見に従った。",
      "furigana": "[彼|かれ]らは[私|わたし]の[意見|い|けん]に[従|したが]った。",
      "translation_zh_CN": "他们屈服于我的看法了。"
    },
    {
      "content": "法律に従うのは我々の義務だ。",
      "furigana": "[法律|ほう|りつ]に[従|したが]うのは[我々|われわれ]の[義務|ぎ|む]だ。",
      "translation_zh_CN": "遵守法律是我们的义务。"
    },
    {
      "content": "彼女の意見に従うほうがいい。",
      "furigana": "[彼女|かの|じょ]の[意見|い|けん]に[従|したが]うほうがいい。",
      "translation_zh_CN": "你最好听取他的意见。"
    },
    {
      "content": "自分の心に従えばいいんだよ。",
      "furigana": "[自分|じ|ぶん]の[心|こころ]に[従|したが]えばいいんだよ。",
      "translation_zh_CN": "顺著你的感觉走吧！"
    },
    {
      "content": "食事療法にきちんと従うように。",
      "furigana": "[食事|しょく|じ][療法|りょう|ほう]にきちんと[従|したが]うように。",
      "translation_zh_CN": "重要的是要遵循严格的饮食控制。"
    },
    {
      "content": "看護婦の指示に従ってください。",
      "furigana": "[看護|かん|ご][婦|ふ]の[指示|し|じ]に[従|したが]ってください。",
      "translation_zh_CN": "请遵循护士的指导。"
    }
  ],
