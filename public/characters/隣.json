  "sentences": [
    {
      "content": "隣どうぞ。",
      "furigana": "[隣|となり]どうぞ。",
      "translation_zh_CN": "请到隔壁来。"
    },
    {
      "content": "隣に座って。",
      "furigana": "[隣|となり]に[座|すわ]って。",
      "translation_zh_CN": "坐在我旁边"
    },
    {
      "content": "隣に住んでいる少年に聞いてみよう。",
      "furigana": "[隣|となり]に[住|す]んでいる[少年|しょう|ねん]に[聞|き]いてみよう。",
      "translation_zh_CN": "我们去问问住在隔壁的男孩吧。"
    },
    {
      "content": "隣の教室は100人入れます。",
      "furigana": "[隣|となり]の[教室|きょう|しつ]は[100|ひゃく][人入|にん|はい]れます。",
      "translation_zh_CN": "隔壁的教室可容纳100人。"
    },
    {
      "content": "隣に女の子が座りました。",
      "furigana": "[隣|となり]に[女|おんな]の[子|こ]が[座|すわ]りました。",
      "translation_zh_CN": "一个女孩坐在我旁边。"
    },
    {
      "content": "となりの客はよく柿食う客だ。",
      "furigana": "となりの[客|きゃく]はよく[柿食|かき|く]う[客|きゃく]だ。",
      "translation_zh_CN": "我旁边的顾客经常吃柿子。"
    },
    {
      "content": "隣町に住んでいます。",
      "furigana": "[隣町|となり|まち]に[住|す]んでいます。",
      "translation_zh_CN": "我住在隔壁的城镇。"
    },
    {
      "content": "隣の下宿人は月３万円ぐらいで暮らしている。",
      "furigana": "[隣|となり]の[下宿人|げ|しゅく|じん]は[月|つき][３万円|さんまんえん]ぐらいで[暮|く]らしている。",
      "translation_zh_CN": "隔壁的房客每月生活费约为3万日元。"
    },
    {
      "content": "彼の隣に座りました。",
      "furigana": "[彼|かれ]の[隣|となり]に[座|すわ]りました。",
      "translation_zh_CN": "我坐在他旁边。"
    },
    {
      "content": "隣近所に対して見栄を張るのは人情ですよ。",
      "furigana": "[隣近所|となり|きん|じょ]に[対|たい]して[見栄|み|え]を[張|は]るのは[人情|にん|じょう]ですよ。",
      "translation_zh_CN": "向邻居炫耀是人的本性。"
    },
    {
      "content": "彼は私の隣人です。",
      "furigana": "[彼|かれ]は[私|わたし]の[隣人|りん|じん]です。",
      "translation_zh_CN": "他是我的一個鄰居。"
    },
    {
      "content": "隣りの人が大嫌い。",
      "furigana": "[隣|とな]りの[人|ひと]が[大嫌|だい|きら]い。",
      "translation_zh_CN": "我讨厌我的邻居。"
    },
    {
      "content": "私たちは隣同士です。",
      "furigana": "[私|わたし]たちは[隣|となり][同士|どう|し]です。",
      "translation_zh_CN": "我們是鄰居。"
    },
    {
      "content": "彼の隣に座ったんだ。",
      "furigana": "[彼|かれ]の[隣|となり]に[座|すわ]ったんだ。",
      "translation_zh_CN": "我坐在他旁边。"
    },
    {
      "content": "隣に座っても良いですか。",
      "furigana": "[隣|となり]に[座|すわ]っても[良|よ]いですか。",
      "translation_zh_CN": "我可以坐在你旁邊嗎？"
    },
    {
      "content": "遠くの兄弟より、良き隣人。",
      "furigana": "[遠|とお]くの[兄弟|きょう|だい]より、[良|よ]き[隣人|りん|じん]。",
      "translation_zh_CN": "远亲不如近邻。"
    },
    {
      "content": "郵便局は図書館の隣にある。",
      "furigana": "[郵便|ゆう|びん][局|きょく]は[図書館|と|しょ|かん]の[隣|となり]にある。",
      "translation_zh_CN": "郵局在圖書館旁邊。"
    },
    {
      "content": "僕は彼の隣に腰を下ろした。",
      "furigana": "[僕|ぼく]は[彼|かれ]の[隣|となり]に[腰|こし]を[下|お]ろした。",
      "translation_zh_CN": "我坐在他旁边。"
    },
    {
      "content": "隣の家との境を示す柵がある。",
      "furigana": "[隣|となり]の[家|いえ]との[境|さかい]を[示|しめ]す[柵|しがらみ]がある。",
      "translation_zh_CN": "我家和邻居家以院子之间的篱笆为分界线。"
    },
    {
      "content": "トムはメアリーの隣に座った。",
      "furigana": "トムはメアリーの[隣|となり]に[座|すわ]った。",
      "translation_zh_CN": "汤姆坐在玛丽旁边。"
    }
  ],
