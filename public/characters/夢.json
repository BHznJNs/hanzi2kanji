  "sentences": [
    {
      "content": "夢じゃなかった。",
      "furigana": "[夢|ゆめ]じゃなかった。",
      "translation_zh_CN": "这不是梦。"
    },
    {
      "content": "夢は逆夢。",
      "furigana": "[夢|ゆめ]は[逆夢|さか|ゆめ]。",
      "translation_zh_CN": "这个梦是一个逆梦。"
    },
    {
      "content": "よい夢を。",
      "furigana": "よい[夢|ゆめ]を。",
      "translation_zh_CN": "甜蜜的梦。"
    },
    {
      "content": "夢をあきらめないで。",
      "furigana": "[夢|ゆめ]をあきらめないで。",
      "translation_zh_CN": "永不停止梦想。"
    },
    {
      "content": "夢を持て。",
      "furigana": "[夢|ゆめ]を[持|も]て。",
      "translation_zh_CN": "做一个梦吧。"
    },
    {
      "content": "夢をつかめ！",
      "furigana": "[夢|ゆめ]をつかめ！",
      "translation_zh_CN": "抓住你的梦想！"
    },
    {
      "content": "万事は夢。",
      "furigana": "[万事|ばん|じ]は[夢|ゆめ]。",
      "translation_zh_CN": "一切都是梦。"
    },
    {
      "content": "夢を見とけ。",
      "furigana": "[夢|ゆめ]を[見|み]とけ。",
      "translation_zh_CN": "梦想继续。"
    },
    {
      "content": "夢は叶った。",
      "furigana": "[夢|ゆめ]は[叶|かな]った。",
      "translation_zh_CN": "梦想成真了。"
    },
    {
      "content": "夢を抱きしめて。",
      "furigana": "[夢|ゆめ]を[抱|だ]きしめて。",
      "translation_zh_CN": "拥抱你的梦想。"
    },
    {
      "content": "私には夢がある。",
      "furigana": "[私|わたし]には[夢|ゆめ]がある。",
      "translation_zh_CN": "我有一个梦想。"
    },
    {
      "content": "悪夢に魘された。",
      "furigana": "[悪夢|あく|む]に[魘|うな]された。",
      "translation_zh_CN": "我做噩梦了。"
    },
    {
      "content": "いい夢を見てね。",
      "furigana": "いい[夢|ゆめ]を[見|み]てね。",
      "translation_zh_CN": "祝你好梦。"
    },
    {
      "content": "猫は夢を見ますか？",
      "furigana": "[猫|ねこ]は[夢|ゆめ]を[見|み]ますか？",
      "translation_zh_CN": "貓會做夢嗎？"
    },
    {
      "content": "ただの夢だった。",
      "furigana": "ただの[夢|ゆめ]だった。",
      "translation_zh_CN": "只是一场梦罢了。"
    },
    {
      "content": "夢は本当になった。",
      "furigana": "[夢|ゆめ]は[本当|ほん|とう]になった。",
      "translation_zh_CN": "我的夢想成真了。"
    },
    {
      "content": "私は彼の夢を見た。",
      "furigana": "[私|わたし]は[彼|かれ]の[夢|ゆめ]を[見|み]た。",
      "translation_zh_CN": "我夢到了他。"
    },
    {
      "content": "よく悪夢を見ます。",
      "furigana": "よく[悪夢|あく|む]を[見|み]ます。",
      "translation_zh_CN": "我经常做噩梦。"
    },
    {
      "content": "あなたに夢中なの。",
      "furigana": "あなたに[夢中|む|ちゅう]なの。",
      "translation_zh_CN": "我为你着迷。"
    },
    {
      "content": "夢を諦めきれない。",
      "furigana": "[夢|ゆめ]を[諦|あきら]めきれない。",
      "translation_zh_CN": "我不能放棄我的夢想。"
    }
  ],
