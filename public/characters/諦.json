  "sentences": [
    {
      "content": "諦めちゃ駄目だ。",
      "furigana": "[諦|あきら]めちゃ[駄目|だ|め]だ。",
      "translation_zh_CN": "不要放棄!"
    },
    {
      "content": "諦めちゃだめよ。",
      "furigana": "[諦|あきら]めちゃだめよ。",
      "translation_zh_CN": "永远不要说永远。"
    },
    {
      "content": "じゃ、諦めるんだね？",
      "furigana": "じゃ、[諦|あきら]めるんだね？",
      "translation_zh_CN": "那你放棄了，是嗎？"
    },
    {
      "content": "夢を諦めきれない。",
      "furigana": "[夢|ゆめ]を[諦|あきら]めきれない。",
      "translation_zh_CN": "我不能放棄我的夢想。"
    },
    {
      "content": "諦めてはいけません。",
      "furigana": "[諦|あきら]めてはいけません。",
      "translation_zh_CN": "不要放棄!"
    },
    {
      "content": "彼女は夢を諦めるしかなかった。",
      "furigana": "[彼女|かの|じょ]は[夢|ゆめ]を[諦|あきら]めるしかなかった。",
      "translation_zh_CN": "她不得不放弃梦想。"
    },
    {
      "content": "彼女は夢を諦めざるをえなかった。",
      "furigana": "[彼女|かの|じょ]は[夢|ゆめ]を[諦|あきら]めざるをえなかった。",
      "translation_zh_CN": "她不得不放弃梦想。"
    },
    {
      "content": "すぐに諦めて昼寝をするかも知れない。",
      "furigana": "すぐに[諦|あきら]めて[昼寝|ひる|ね]をするかも[知|し]れない。",
      "translation_zh_CN": "也许我会马上放弃然后去打盹。"
    }
  ],
