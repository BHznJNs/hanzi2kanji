  "sentences": [
    {
      "content": "彼は泳ぎに行こうと勧めた。",
      "furigana": "[彼|かれ]は[泳|およ]ぎに[行|い]こうと[勧|すす]めた。",
      "translation_zh_CN": "他提议大家一起去游泳。"
    },
    {
      "content": "医者からは禁煙を勧められた。",
      "furigana": "[医者|い|しゃ]からは[禁煙|きん|えん]を[勧|すす]められた。",
      "translation_zh_CN": "醫生勸我戒煙。"
    },
    {
      "content": "私は彼女に朝の列車に乗るように勧めた。",
      "furigana": "[私|わたし]は[彼女|かの|じょ]に[朝|あさ]の[列車|れっ|しゃ]に[乗|の]るように[勧|すす]めた。",
      "translation_zh_CN": "我建議她去乘早車。"
    },
    {
      "content": "その映画をみるように勧めたのはあなたよ。",
      "furigana": "その[映画|えい|が]をみるように[勧|すす]めたのはあなたよ。",
      "translation_zh_CN": "是你提议去看这个电影的。"
    },
    {
      "content": "あの映画を勧めてくれたの、お前だったじゃん。",
      "furigana": "あの[映画|えい|が]を[勧|すす]めてくれたの、お[前|まえ]だったじゃん。",
      "translation_zh_CN": "是你提议去看这个电影的。"
    },
    {
      "content": "先生は僕にシェイクスピアを読むよう勧めてくれた。",
      "furigana": "[先生|せん|せい]は[僕|ぼく]にシェイクスピアを[読|よ]むよう[勧|すす]めてくれた。",
      "translation_zh_CN": "老師建議我讀莎士比亞。"
    }
  ],
