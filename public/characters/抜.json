  "sentences": [
    {
      "content": "風邪が抜けない。",
      "furigana": "[風邪|かぜ]が[抜|ぬ]けない。",
      "translation_zh_CN": "我的感冒怎么也不好。"
    },
    {
      "content": "歯が１本抜けた。",
      "furigana": "[歯|は]が[１|いっ][本|ぽん][抜|ぬ]けた。",
      "translation_zh_CN": "我掉了颗牙。"
    },
    {
      "content": "気が抜けちゃったよ。",
      "furigana": "[気|き]が[抜|ぬ]けちゃったよ。",
      "translation_zh_CN": "我累死了。"
    },
    {
      "content": "彼は約束を守り抜いた。",
      "furigana": "[彼|かれ]は[約束|やく|そく]を[守|まも]り[抜|ぬ]いた。",
      "translation_zh_CN": "他信守了承诺."
    },
    {
      "content": "虫歯を抜いてもらった。",
      "furigana": "[虫歯|むし|ば]を[抜|ぬ]いてもらった。",
      "translation_zh_CN": "我把蛀牙拔掉了。"
    },
    {
      "content": "彼女は庭の雑草を抜いた。",
      "furigana": "[彼女|かの|じょ]は[庭|にわ]の[雑草|ざっ|そう]を[抜|ぬ]いた。",
      "translation_zh_CN": "她给花园除了草。"
    },
    {
      "content": "一週間メシ抜きだったよ。",
      "furigana": "[一|いっ][週間|しゅう|かん]メシ[抜|ぬ]きだったよ。",
      "translation_zh_CN": "我一星期没带食物去。"
    },
    {
      "content": "トムは教室を抜け出した。",
      "furigana": "トムは[教室|きょう|しつ]を[抜|ぬ]け[出|だ]した。",
      "translation_zh_CN": "汤姆溜出了教室。"
    },
    {
      "content": "あなたの髭を抜きたいです。",
      "furigana": "あなたの[髭|ひげ]を[抜|ぬ]きたいです。",
      "translation_zh_CN": "我想拔你的胡须。"
    },
    {
      "content": "タイヤの空気が抜けてるよ。",
      "furigana": "タイヤの[空気|くう|き]が[抜|ぬ]けてるよ。",
      "translation_zh_CN": "轮胎漏气了。"
    }
  ],
