  "sentences": [
    {
      "content": "習うより慣れろ。",
      "furigana": "[習|なら]うより[慣|な]れろ。",
      "translation_zh_CN": "熟能生巧。"
    },
    {
      "content": "彼は旅慣れている。",
      "furigana": "[彼|かれ]は[旅|たび][慣|な]れている。",
      "translation_zh_CN": "他習慣了旅行。"
    },
    {
      "content": "彼らは同じ習慣をもつ。",
      "furigana": "[彼|かれ]らは[同|おな]じ[習慣|しゅう|かん]をもつ。",
      "translation_zh_CN": "他們有相同的嗜好。"
    },
    {
      "content": "習慣の力は偉大である。",
      "furigana": "[習慣|しゅう|かん]の[力|ちから]は[偉大|い|だい]である。",
      "translation_zh_CN": "习惯的力量是伟大的。"
    },
    {
      "content": "寮生活には慣れましたか。",
      "furigana": "[寮|りょう][生活|せい|かつ]には[慣|な]れましたか。",
      "translation_zh_CN": "你習慣在宿舍生活了嗎？"
    },
    {
      "content": "習慣は第二の天性である。",
      "furigana": "[習慣|しゅう|かん]は[第|だい][二|に]の[天性|てん|せい]である。",
      "translation_zh_CN": "习惯是第二天性。"
    },
    {
      "content": "夜更かしには慣れてます。",
      "furigana": "[夜|よ][更|ふ]かしには[慣|な]れてます。",
      "translation_zh_CN": "我习惯晚睡。"
    },
    {
      "content": "大丈夫、じきに慣れますよ。",
      "furigana": "[大丈夫|だい|じょう|ぶ]、じきに[慣|な]れますよ。",
      "translation_zh_CN": "你很快就會習慣的。"
    },
    {
      "content": "早起きには慣れているんだ。",
      "furigana": "[早起|はや|お]きには[慣|な]れているんだ。",
      "translation_zh_CN": "我習慣早起。"
    },
    {
      "content": "日本食にはすぐに慣れますよ。",
      "furigana": "[日本|に|ほん][食|しょく]にはすぐに[慣|な]れますよ。",
      "translation_zh_CN": "你很快就会习惯吃日本料理了。"
    }
  ],
