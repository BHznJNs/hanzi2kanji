  "sentences": [
    {
      "content": "彼女はビールを常飲し始めた。",
      "furigana": "[彼女|かの|じょ]はビールを[常飲|じょう|いん]し[始|はじ]めた。",
      "translation_zh_CN": "她开始定期喝啤酒。"
    },
    {
      "content": "何を飲みますか。",
      "furigana": "[何|なに]を[飲|の]みますか。",
      "translation_zh_CN": "你想喝点甚么？"
    },
    {
      "content": "何か飲みますか。",
      "furigana": "[何|なに]か[飲|の]みますか。",
      "translation_zh_CN": "你想要什么喝的吗？"
    },
    {
      "content": "飲んだら乗るな。",
      "furigana": "[飲|の]んだら[乗|の]るな。",
      "translation_zh_CN": "酒后别开车 。"
    },
    {
      "content": "お飲み物はいかが？",
      "furigana": "お[飲|の]み[物|もの]はいかが？",
      "translation_zh_CN": "你想要什么喝的吗？"
    },
    {
      "content": "コーヒーを飲むの？",
      "furigana": "コーヒーを[飲|の]むの？",
      "translation_zh_CN": "你喝咖啡吗？"
    },
    {
      "content": "牛乳は飲めない。",
      "furigana": "[牛乳|ぎゅう|にゅう]は[飲|の]めない。",
      "translation_zh_CN": "我不可以喝牛奶。"
    },
    {
      "content": "この水は飲めるの？",
      "furigana": "この[水|みず]は[飲|の]めるの？",
      "translation_zh_CN": "这水能喝吗？"
    },
    {
      "content": "薬はもう飲んだの？",
      "furigana": "[薬|くすり]はもう[飲|の]んだの？",
      "translation_zh_CN": "您已经吃过药了吗？"
    },
    {
      "content": "何か飲み物をくれ。",
      "furigana": "[何|なに]か[飲|の]み[物|もの]をくれ。",
      "translation_zh_CN": "给我点喝的。"
    },
    {
      "content": "お酒は飲みますか。",
      "furigana": "お[酒|さけ]は[飲|の]みますか。",
      "translation_zh_CN": "你喝酒吗？"
    }
  ],
