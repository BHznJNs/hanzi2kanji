  "sentences": [
    {
      "content": "稲の収穫期にはお祭りをする。",
      "furigana": "[稲|いね]の[収穫期|しゅう|かく|き]にはお[祭|まつ]りをする。",
      "translation_zh_CN": "稻米收获季节会举办祭典。"
    },
    {
      "content": "彼は稲を育てている。",
      "furigana": "[彼|かれ]は[稲|いね]を[育|そだ]てている。",
      "translation_zh_CN": "他正在种植水稻。"
    },
    {
      "content": "稲の取り入れはもう済んだ。",
      "furigana": "[稲|いね]の[取|と]り[入|い]れはもう[済|す]んだ。",
      "translation_zh_CN": "水稻的收割工作已经完成。"
    },
    {
      "content": "農夫達は稲を植えていた。",
      "furigana": "[農夫達|のう|ふ|たち]は[稲|いね]を[植|う]えていた。",
      "translation_zh_CN": "农民们正在种植水稻。"
    },
    {
      "content": "いいよ、6枚ね ",
      "furigana": "いいよ、[6|ろく][枚|まい]ね ",
      "translation_zh_CN": "好的，6块。"
    },
    {
      "content": "今年は稲の育ちがいい。",
      "furigana": "[今年|こ|とし]は[稲|いね]の[育|そだ]ちがいい。",
      "translation_zh_CN": "今年水稻长势良好。"
    },
    {
      "content": "寒波が稲の発育を遅らせた。",
      "furigana": "[寒波|かん|ぱ]が[稲|いね]の[発育|はつ|いく]を[遅|おく]らせた。",
      "translation_zh_CN": "寒潮推迟了水稻的生长。"
    },
    {
      "content": "トムって、失礼ね。",
      "furigana": "トムって、[失礼|しつ|れい]ね。",
      "translation_zh_CN": "汤姆，对不起。"
    },
    {
      "content": "稲は温暖な国で育ちます。",
      "furigana": "[稲|いね]は[温暖|おん|だん]な[国|くに]で[育|そだ]ちます。",
      "translation_zh_CN": "水稻生长在温暖的国家。"
    },
    {
      "content": "稲は温暖な地域で育ちます。",
      "furigana": "[稲|いね]は[温暖|おん|だん]な[地域|ち|いき]で[育|そだ]ちます。",
      "translation_zh_CN": "水稻生长在温暖地区。"
    },
    {
      "content": "彼は稲を育てている。",
      "furigana": "[彼|かれ]は[稲|いね]を[育|そだ]てている。",
      "translation_zh_CN": "他种米。"
    },
    {
      "content": "稲は温暖な国で育ちます。",
      "furigana": "[稲|いね]は[温暖|おん|だん]な[国|くに]で[育|そだ]ちます。",
      "translation_zh_CN": "稻米生長在溫暖的國家。"
    },
    {
      "content": "稲は温暖な地域で育ちます。",
      "furigana": "[稲|いね]は[温暖|おん|だん]な[地域|ち|いき]で[育|そだ]ちます。",
      "translation_zh_CN": "水稻生長在溫暖的氣候。"
    },
    {
      "content": "実るほど頭の下がる稲穂かな。",
      "furigana": "[実|みの]るほど[頭|あたま]の[下|さ]がる[稲穂|いな|ほ]かな。",
      "translation_zh_CN": "稻穗越飽滿越低頭。"
    },
    {
      "content": "稲妻は普通、雷鳴の前に光る。",
      "furigana": "[稲妻|いな|づま]は[普通|ふ|つう]、[雷鳴|らい|めい]の[前|まえ]に[光|ひか]る。",
      "translation_zh_CN": "閃電常常伴隨著雷鳴。"
    },
    {
      "content": "稲は雨の多い地域で作られる。",
      "furigana": "[稲|いね]は[雨|あめ]の[多|おお]い[地域|ち|いき]で[作|つく]られる。",
      "translation_zh_CN": "人們在多雨地區種植水稻。"
    },
    {
      "content": "実るほど頭を垂れる稲穂かな。",
      "furigana": "[実|みの]るほど[頭|こうべ]を[垂|た]れる[稲穂|いな|ほ]かな。",
      "translation_zh_CN": "稻穗越飽滿越低頭。"
    },
    {
      "content": "稲作は雨の多い地域で行われている。",
      "furigana": "[稲作|いな|さく]は[雨|あめ]の[多|おお]い[地域|ち|いき]で[行|おこな]われている。",
      "translation_zh_CN": "人們在多雨地區種植水稻。"
    },
    {
      "content": "稲光はたいてい雷の音がした後で光る。",
      "furigana": "[稲光|いな|びかり]はたいてい[雷|かみなり]の[音|おと]がした[後|のち]で[光|ひか]る。",
      "translation_zh_CN": "閃電常常伴隨著雷鳴。"
    },
    {
      "content": "雷鳴が聞こえるよりも先に稲光が見えるのは、光が音よりも早く伝わるからである。",
      "furigana": "[雷鳴|らい|めい]が[聞|き]こえるよりも[先|さき]に[稲光|いな|びかり]が[見|み]えるのは、[光|ひかり]が[音|おと]よりも[早|はや]く[伝|つた]わるからである。",
      "translation_zh_CN": "光傳播得比聲音快，所以我們先看到閃電，然後才會聽見雷聲。"
    }
