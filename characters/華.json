  "sentences": [
    {
      "content": "華金だ！",
      "furigana": "[華金|はな|きん]だ！",
      "translation_zh_CN": "是卡金！"
    },
    {
      "content": "お華を習ってるの。",
      "furigana": "お[華|はな]を[習|なら]ってるの。",
      "translation_zh_CN": "我正在学习花。"
    },
    {
      "content": "頼めば、その華はもらえる。",
      "furigana": "[頼|たの]めば、その[華|はな]はもらえる。",
      "translation_zh_CN": "如果你要求它，你就会收到它。"
    },
    {
      "content": "唇に熱の華ができちゃった。",
      "furigana": "[唇|くちびる]に[熱|ねつ]の[華|はな]ができちゃった。",
      "translation_zh_CN": "一朵热花在我的唇上形成。"
    },
    {
      "content": "横浜の中華飯店へ食べに行きました。",
      "furigana": "[横浜|よこ|はま]の[中華飯店|ちゅう|か|はん|てん]へ[食|た]べに[行|い]きました。",
      "translation_zh_CN": "我去横滨的一家中国餐馆吃饭。"
    },
    {
      "content": "「こりゃ放火だぜ」「芳華って誰？」",
      "furigana": "「こりゃ[放火|ほう|か]だぜ」「[芳華|ほう|か]って[誰|だれ]？」",
      "translation_zh_CN": "“这是纵火案。”"
    },
    {
      "content": "エミリーさんはお茶やお華にとても関心をお持ちなの。",
      "furigana": "エミリーさんはお[茶|ちゃ]やお[華|はな]にとても[関心|かん|しん]をお[持|も]ちなの。",
      "translation_zh_CN": "艾米丽对茶和花很感兴趣。"
    },
    {
      "content": "水は華氏３２度で凍る。",
      "furigana": "[水|みず]は[華氏|か|し][３|][２|][度|ど]で[凍|こお]る。",
      "translation_zh_CN": "水在华氏32度结成冰。"
    },
    {
      "content": "中華料理が好きですか？",
      "furigana": "[中華|ちゅう|か][料理|りょう|り]が[好|す]きですか？",
      "translation_zh_CN": "你喜欢中国菜吗？"
    },
    {
      "content": "中華料理が好きですか。",
      "furigana": "[中華|ちゅう|か][料理|りょう|り]が[好|す]きですか。",
      "translation_zh_CN": "你喜欢中国菜吗？"
    },
    {
      "content": "中華は好きか聞かれたよ。",
      "furigana": "[中華|ちゅう|か]は[好|す]きか[聞|き]かれたよ。",
      "translation_zh_CN": "他问我是否喜欢中国菜。"
    },
    {
      "content": "華道は日本の文化の一つです。",
      "furigana": "[華道|か|どう]は[日本|にっ|ぽん]の[文化|ぶん|か]の[一|ひと]つです。",
      "translation_zh_CN": "花道是日本文化的一部分。"
    },
    {
      "content": "私たちは中華料理を注文した。",
      "furigana": "[私|わたし]たちは[中華|ちゅう|か][料理|りょう|り]を[注文|ちゅう|もん]した。",
      "translation_zh_CN": "我们点了中餐。"
    },
    {
      "content": "彼女は豪華な絹の服を着ていた。",
      "furigana": "[彼女|かの|じょ]は[豪華|ごう|か]な[絹|きぬ]の[服|ふく]を[着|き]ていた。",
      "translation_zh_CN": "她穿著华丽的丝绸服饰。"
    },
    {
      "content": "彼は中華料理がすきかたずねた。",
      "furigana": "[彼|かれ]は[中華|ちゅう|か][料理|りょう|り]がすきかたずねた。",
      "translation_zh_CN": "他问我是否喜欢中国菜。"
    },
    {
      "content": "彼は私に中華料理が好きかどうかたずねた。",
      "furigana": "[彼|かれ]は[私|わたし]に[中華|ちゅう|か][料理|りょう|り]が[好|す]きかどうかたずねた。",
      "translation_zh_CN": "他问我是否喜欢中国菜。"
    },
    {
      "content": "フランスには本物の中華料理店はありますか？",
      "furigana": "フランスには[本物|ほん|もの]の[中華|ちゅう|か][料理|りょう|り][店|てん]はありますか？",
      "translation_zh_CN": "法国有正宗的中餐吗？"
    }
  ],
