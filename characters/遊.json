  "sentences": [
    {
      "content": "私は庭で遊びます。",
      "furigana": "[私|わたし]は[庭|にわ]で[遊|あそ]びます。",
      "translation_zh_CN": "我在花园里玩。"
    },
    {
      "content": "さあ、一緒に遊ぼう。",
      "furigana": "さあ、[一緒|いっ|しょ]に[遊|あそ]ぼう。",
      "translation_zh_CN": "来一起玩吧！"
    },
    {
      "content": "ゲームして遊ぼうよ。",
      "furigana": "ゲームして[遊|あそ]ぼうよ。",
      "translation_zh_CN": "让我们玩一款游戏吧。"
    },
    {
      "content": "明日遊びに来て下さい。",
      "furigana": "[明日|あした][遊|あそ]びに[来|き]て[下|くだ]さい。",
      "translation_zh_CN": "明天来看看我。"
    },
    {
      "content": "明日遊びに行くからね。",
      "furigana": "[明日|あした][遊|あそ]びに[行|い]くからね。",
      "translation_zh_CN": "我明天會來看你。"
    },
    {
      "content": "妹は人形で遊んでいる。",
      "furigana": "[妹|いもうと]は[人形|にん|ぎょう]で[遊|あそ]んでいる。",
      "translation_zh_CN": "我妹妹正在玩洋娃娃。"
    },
    {
      "content": "明日遊びにいらっしゃい。",
      "furigana": "[明日|あした][遊|あそ]びにいらっしゃい。",
      "translation_zh_CN": "明天来看看我。"
    },
    {
      "content": "道路で遊んではいけません。",
      "furigana": "[道路|どう|ろ]で[遊|あそ]んではいけません。",
      "translation_zh_CN": "別在馬路上玩。"
    },
    {
      "content": "子供達は外に遊びに行った。",
      "furigana": "[子供|こ|ども][達|たち]は[外|そと]に[遊|あそ]びに[行|い]った。",
      "translation_zh_CN": "孩子們出去玩了。"
    },
    {
      "content": "今晩リサと遊びに行くんだ。",
      "furigana": "[今晩|こん|ばん]リサと[遊|あそ]びに[行|い]くんだ。",
      "translation_zh_CN": "我今晚要和麗莎出去。"
    }
