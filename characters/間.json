  "sentences": [
    {
      "content": "もう少しの間頑張れるかい？",
      "furigana": "もう[少|すこ]しの[間頑張|あいだ|がん|ば]れるかい？",
      "translation_zh_CN": "你能再坚持一会儿吗？"
    },
    {
      "content": "私は長い間待たされた。",
      "furigana": "[私|わたし]は[長|なが]い[間待|あいだ|ま]たされた。",
      "translation_zh_CN": "我不得不等待很长时间。"
    },
    {
      "content": "嵐の間ずっと寝ていた。",
      "furigana": "[嵐|あらし]の[間|かん]ずっと[寝|ね]ていた。",
      "translation_zh_CN": "整个暴风雨我都在睡觉。"
    },
    {
      "content": "一瞬の間が生まれた。",
      "furigana": "[一瞬|いっ|しゅん]の[間|ま]が[生|う]まれた。",
      "translation_zh_CN": "短暂的停顿诞生了。"
    },
    {
      "content": "彼は長い間待っていた。",
      "furigana": "[彼|かれ]は[長|なが]い[間待|あいだ|ま]っていた。",
      "translation_zh_CN": "他已经等了很久了。"
    },
    {
      "content": "長い間笑ってない。",
      "furigana": "[長|なが]い[間笑|あいだ|わら]ってない。",
      "translation_zh_CN": "我已经很久没有笑过了。"
    },
    {
      "content": "靴は新しい間は硬い。",
      "furigana": "[靴|くつ]は[新|あたら]しい[間|ま]は[硬|かた]い。",
      "translation_zh_CN": "新鞋子很硬。"
    },
    {
      "content": "しかし男の子は長い間来ませんでした。",
      "furigana": "しかし[男|おとこ]の[子|こ]は[長|なが]い[間来|ま|き]ませんでした。",
      "translation_zh_CN": "但男孩很久都没有来。"
    },
    {
      "content": "眠っている間に財布を盗まれた。",
      "furigana": "[眠|ねむ]っている[間|あいだ]に[財布|さい|ふ]を[盗|ぬす]まれた。",
      "translation_zh_CN": "我的钱包在我睡觉时被偷了。"
    },
    {
      "content": "雪は瞬く間に消えてしまった。",
      "furigana": "[雪|ゆき]は[瞬|またた]く[間|ま]に[消|き]えてしまった。",
      "translation_zh_CN": "雪瞬间就消失了。"
    },
    {
      "content": "嵐の間停電した。",
      "furigana": "[嵐|あらし]の[間|ま][停電|てい|でん]した。",
      "translation_zh_CN": "暴风雨中，停电了。"
    },
    {
      "content": "人間は必ず死ぬ。",
      "furigana": "[人間|にん|げん]は[必|かなら]ず[死|し]ぬ。",
      "translation_zh_CN": "人必定要死的。"
    },
    {
      "content": "寝る時間ですよ。",
      "furigana": "[寝|ね]る[時間|じ|かん]ですよ。",
      "translation_zh_CN": "睡觉时间到了。"
    },
    {
      "content": "出発する時間だ。",
      "furigana": "[出発|しゅっ|ぱつ]する[時間|じ|かん]だ。",
      "translation_zh_CN": "是时候离开了。"
    },
    {
      "content": "出かける時間よ。",
      "furigana": "[出|で]かける[時間|じ|かん]よ。",
      "translation_zh_CN": "该走了。"
    },
    {
      "content": "私は時間がない。",
      "furigana": "[私|わたし]は[時間|じ|かん]がない。",
      "translation_zh_CN": "我没时间。"
    },
    {
      "content": "もう時間はない。",
      "furigana": "もう[時間|じ|かん]はない。",
      "translation_zh_CN": "我们没时间。"
    },
    {
      "content": "まだ時間がある。",
      "furigana": "まだ[時間|じ|かん]がある。",
      "translation_zh_CN": "我们仍然有更多的时间。"
    },
    {
      "content": "そろそろ時間だ。",
      "furigana": "そろそろ[時間|じ|かん]だ。",
      "translation_zh_CN": "时间到了。"
    },
    {
      "content": "これらは人間だ。",
      "furigana": "これらは[人間|にん|げん]だ。",
      "translation_zh_CN": "这些是人。"
    }
