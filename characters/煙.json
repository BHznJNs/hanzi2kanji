  "sentences": [
    {
      "content": "あの煙を見て。",
      "furigana": "あの[煙|けむり]を[見|み]て。",
      "translation_zh_CN": "看看那烟雾。"
    },
    {
      "content": "煙でむせた。",
      "furigana": "[煙|けむり]でむせた。",
      "translation_zh_CN": "我被烟呛到了。"
    },
    {
      "content": "あの煙を見なさい。",
      "furigana": "あの[煙|けむり]を[見|み]なさい。",
      "translation_zh_CN": "看看那烟雾。"
    },
    {
      "content": "煙のない火はない。",
      "furigana": "[煙|けむり]のない[火|ひ]はない。",
      "translation_zh_CN": "没有烟就没有火。"
    },
    {
      "content": "煙突から煙が立っていた。",
      "furigana": "[煙突|えん|とつ]から[煙|けむり]が[立|た]っていた。",
      "translation_zh_CN": "烟从烟囱里冒出来。"
    },
    {
      "content": "煙突から煙が出ている。",
      "furigana": "[煙突|えん|とつ]から[煙|けむり]が[出|で]ている。",
      "translation_zh_CN": "烟从烟囱里冒出来。"
    },
    {
      "content": "煙が空へ昇っている。",
      "furigana": "[煙|けむり]が[空|そら]へ[昇|のぼ]っている。",
      "translation_zh_CN": "烟雾升腾到天空。"
    },
    {
      "content": "煙は空中へと舞い上がった。",
      "furigana": "[煙|けむり]は[空中|くう|ちゅう]へと[舞|ま]い[上|あ]がった。",
      "translation_zh_CN": "烟雾升入空中。"
    },
    {
      "content": "煙が風に吹き流された。",
      "furigana": "[煙|けむり]が[風|かぜ]に[吹|ふ]き[流|なが]された。",
      "translation_zh_CN": "烟雾被风吹散了。"
    },
    {
      "content": "煙突から煙がもくもくと出ていた。",
      "furigana": "[煙突|えん|とつ]から[煙|けむり]がもくもくと[出|で]ていた。",
      "translation_zh_CN": "烟从烟囱里冒出来。"
    },
    {
      "content": "彼女は禁煙した。",
      "furigana": "[彼女|かの|じょ]は[禁煙|きん|えん]した。",
      "translation_zh_CN": "她停下来抽烟。"
    },
    {
      "content": "喫煙は絶対禁止。",
      "furigana": "[喫煙|きつ|えん]は[絶対|ぜっ|たい][禁止|きん|し]。",
      "translation_zh_CN": "严禁吸烟。"
    },
    {
      "content": "ここは禁煙です。",
      "furigana": "ここは[禁煙|きん|えん]です。",
      "translation_zh_CN": "这里不允许抽烟。"
    },
    {
      "content": "喫煙は人を殺す。",
      "furigana": "[喫煙|きつ|えん]は[人|ひと]を[殺|ころ]す。",
      "translation_zh_CN": "吸烟有害。"
    },
    {
      "content": "父は煙草を止めた。",
      "furigana": "[父|ちち]は[煙草|たばこ]を[止|や]めた。",
      "translation_zh_CN": "我父亲戒烟了。"
    },
    {
      "content": "彼は喫煙をやめた。",
      "furigana": "[彼|かれ]は[喫煙|きつ|えん]をやめた。",
      "translation_zh_CN": "他把烟戒了。"
    },
    {
      "content": "彼は煙草をやめた。",
      "furigana": "[彼|かれ]は[煙草|たばこ]をやめた。",
      "translation_zh_CN": "他把烟戒了。"
    },
    {
      "content": "喫煙は健康に悪い。",
      "furigana": "[喫煙|きつ|えん]は[健康|けん|こう]に[悪|わる]い。",
      "translation_zh_CN": "吸烟对你的健康不好。"
    },
    {
      "content": "煙草を吸いますか。",
      "furigana": "[煙草|たばこ]を[吸|す]いますか。",
      "translation_zh_CN": "你吸烟吗?"
    },
    {
      "content": "ここでは禁煙です。",
      "furigana": "ここでは[禁煙|きん|えん]です。",
      "translation_zh_CN": "你不应在这里吸烟。"
    }
