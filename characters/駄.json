  "sentences": [
    {
      "content": "諦めちゃ駄目だ。",
      "furigana": "[諦|あきら]めちゃ[駄目|だ|め]だ。",
      "translation_zh_CN": "不要放弃!"
    },
    {
      "content": "これは駄洒落です。",
      "furigana": "これは[駄洒落|だ|じゃ|れ]です。",
      "translation_zh_CN": "这是个文字游戏。"
    },
    {
      "content": "泣いても無駄です。",
      "furigana": "[泣|な]いても[無駄|む|だ]です。",
      "translation_zh_CN": "哭是无济于事的。"
    },
    {
      "content": "外出は駄目ですよ。",
      "furigana": "[外出|がい|しゅつ]は[駄目|だ|め]ですよ。",
      "translation_zh_CN": "你不可以出去。"
    },
    {
      "content": "無駄骨を折るだけだ。",
      "furigana": "[無駄骨|む|だ|ぼね]を[折|お]るだけだ。",
      "translation_zh_CN": "这好比大海捞针。"
    },
    {
      "content": "彼と議論しても無駄だ。",
      "furigana": "[彼|かれ]と[議論|ぎ|ろん]しても[無駄|む|だ]だ。",
      "translation_zh_CN": "跟他吵没用。"
    },
    {
      "content": "言い訳をしても無駄だ。",
      "furigana": "[言|い]い[訳|わけ]をしても[無駄|む|だ]だ。",
      "translation_zh_CN": "借口是没用的。"
    },
    {
      "content": "それは時間の無駄です。",
      "furigana": "それは[時間|じ|かん]の[無駄|む|だ]です。",
      "translation_zh_CN": "这是浪费时间。"
    },
    {
      "content": "今更後悔しても無駄だ。",
      "furigana": "[今更|いま|さら][後悔|こう|かい]しても[無駄|む|だ]だ。",
      "translation_zh_CN": "现在后悔也没用了。"
    },
    {
      "content": "時間を無駄にするなよ。",
      "furigana": "[時間|じ|かん]を[無駄|む|だ]にするなよ。",
      "translation_zh_CN": "不要浪费时间。"
    }
