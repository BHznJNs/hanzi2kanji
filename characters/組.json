  "sentences": [
    {
      "content": "さい1組持ってる？",
      "furigana": "さい1[組持|くみ|も]ってる？",
      "translation_zh_CN": "你有一双赛吗？"
    },
    {
      "content": "何組なの？",
      "furigana": "[何組|なん|くみ]なの？",
      "translation_zh_CN": "有多少组？"
    },
    {
      "content": "私は手袋を１組買った。",
      "furigana": "[私|わたし]は[手袋|て|ぶくろ]を１[組買|くみ|か]った。",
      "translation_zh_CN": "我买了一双手套。"
    },
    {
      "content": "彼はその組の首席だ。",
      "furigana": "[彼|かれ]はその[組|くみ]の[首席|しゅ|せき]だ。",
      "translation_zh_CN": "他是小组的组长。"
    },
    {
      "content": "トムって、何組なの？",
      "furigana": "トムって、[何組|なん|くみ]なの？",
      "translation_zh_CN": "汤姆属于多少个组？"
    },
    {
      "content": "彼は新しい手袋を１組買った。",
      "furigana": "[彼|かれ]は[新|あたら]しい[手袋|て|ぶくろ]を１[組買|くみ|か]った。",
      "translation_zh_CN": "他买了一副新手套。"
    },
    {
      "content": "そう、2組のカップルでデートするの",
      "furigana": "そう、[2|ふた][組|くみ]の[カップル|かっぷる]で[デ|で][ー|ー][ト|と]するの",
      "translation_zh_CN": "是的，两对情侣去约会。"
    },
    {
      "content": "私は彼の思想には組しない。",
      "furigana": "[私|わたし]は[彼|かれ]の[思想|し|そう]には[組|くみ]しない。",
      "translation_zh_CN": "我不同意他的想法。"
    },
    {
      "content": "きのうここで結婚式が２組あった。",
      "furigana": "きのうここで[結婚式|けっ|こん|しき]が２[組|くみ]あった。",
      "translation_zh_CN": "昨天这里举行了两场婚礼。"
    },
    {
      "content": "ホワイト先生が我々の組を受け持っています。",
      "furigana": "ホワイト[先生|せん|せい]が[我々|われわれ]の[組|くみ]を[受|う]け[持|も]っています。",
      "translation_zh_CN": "怀特先生是我们小组的负责人。"
    },
    {
      "content": "この番組は再放送だ。",
      "furigana": "この[番組|ばん|ぐみ]は[再|さい][放送|ほう|そう]だ。",
      "translation_zh_CN": "这个节目是重播。"
    },
    {
      "content": "もうこの番組には飽きた。",
      "furigana": "もうこの[番組|ばん|ぐみ]には[飽|あ]きた。",
      "translation_zh_CN": "我已经受够了这个计划。"
    },
    {
      "content": "その番組は今放送中です。",
      "furigana": "その[番組|ばん|ぐみ]は[今|いま][放送|ほう|そう][中|ちゅう]です。",
      "translation_zh_CN": "那個節目現在正在播出。"
    },
    {
      "content": "ジョンは本棚を組み立てた。",
      "furigana": "ジョンは[本棚|ほん|だな]を[組|く]み[立|た]てた。",
      "translation_zh_CN": "约翰组装了书架。"
    },
    {
      "content": "彼女はその問題に取り組んでいる。",
      "furigana": "[彼女|かの|じょ]はその[問題|もん|だい]に[取|と]り[組|く]んでいる。",
      "translation_zh_CN": "她在处理问题。"
    },
    {
      "content": "昨日の夜、私の番組は聞いてくれた？",
      "furigana": "[昨日|きのう]の[夜|よる]、[私|わたし]の[番組|ばん|ぐみ]は[聞|き]いてくれた？",
      "translation_zh_CN": "你听了我昨天晚上的节目吗？"
    },
    {
      "content": "ベティは脚を組んでいすに座っていた。",
      "furigana": "ベティは[脚|あし]を[組|く]んでいすに[座|すわ]っていた。",
      "translation_zh_CN": "Betty 翹起雙腿，在椅子上坐著。"
    },
    {
      "content": "パーシーという人が君の組にいますか。",
      "furigana": "パーシーという[人|ひと]が[君|きみ]の[組|くみ]にいますか。",
      "translation_zh_CN": "你班裏有沒有一個叫做珀西的人？"
    },
    {
      "content": "テントを組み立てるのに３０分かかった。",
      "furigana": "テントを[組|く]み[立|た]てるのに[３|さん][０|じゅっ][分|ぷん]かかった。",
      "translation_zh_CN": "搭帳篷花了我們半小時。"
    },
    {
      "content": "この機械の仕組みを説明してもらえますか？",
      "furigana": "この[機械|き|かい]の[仕組|し|く]みを[説明|せつ|めい]してもらえますか？",
      "translation_zh_CN": "你可以說明一下這台機器是怎樣運作的嗎？"
    }
