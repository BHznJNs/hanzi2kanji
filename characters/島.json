  "sentences": [
    {
      "content": "島が見えてきた。",
      "furigana": "[島|しま]が[見|み]えてきた。",
      "translation_zh_CN": "岛屿映入眼帘。"
    },
    {
      "content": "小さな島が見えてきた。",
      "furigana": "[小|ちい]さな[島|しま]が[見|み]えてきた。",
      "translation_zh_CN": "一座小岛映入眼帘。"
    },
    {
      "content": "遠くに島が見えた。",
      "furigana": "[遠|とお]くに[島|しま]が[見|み]えた。",
      "translation_zh_CN": "我可以看到远处有一座岛屿。"
    },
    {
      "content": "イギリスは島です。",
      "furigana": "イギリスは[島|しま]です。",
      "translation_zh_CN": "英国是一个岛国。"
    },
    {
      "content": "オーストラリアって、島？",
      "furigana": "オーストラリアって、[島|しま]？",
      "translation_zh_CN": "澳大利亚是一个岛国吗？"
    },
    {
      "content": "遠方にその島が見える。",
      "furigana": "[遠方|えん|ぽう]にその[島|しま]が[見|み]える。",
      "translation_zh_CN": "远处可以看到小岛。"
    },
    {
      "content": "ギリシャは島が多い。",
      "furigana": "ギリシャは[島|しま]が[多|おお]い。",
      "translation_zh_CN": "希腊有许多岛屿。"
    },
    {
      "content": "ナポレオンはセントヘレナ島へ流された。",
      "furigana": "ナポレオンはセントヘレナ[島|とう]へ[流|なが]された。",
      "translation_zh_CN": "拿破仑被流放到圣赫勒拿岛。"
    },
    {
      "content": "カリブ海には島が多い。",
      "furigana": "カリブ[海|かい]には[島|しま]が[多|おお]い。",
      "translation_zh_CN": "加勒比海有许多岛屿。"
    },
    {
      "content": "その島は一年中あたたかい。",
      "furigana": "その[島|しま]は[一年中|いち|ねん|じゅう]あたたかい。",
      "translation_zh_CN": "岛上一年四季气候温暖。"
    },
    {
      "content": "島が見えてきた。",
      "furigana": "[島|しま]が[見|み]えてきた。",
      "translation_zh_CN": "一座岛屿映入眼帘。"
    },
    {
      "content": "海には島がある。",
      "furigana": "[海|うみ]には[島|しま]がある。",
      "translation_zh_CN": "海里有岛。"
    },
    {
      "content": "海の上に島がある。",
      "furigana": "[海|うみ]の[上|うえ]に[島|しま]がある。",
      "translation_zh_CN": "有海岛。"
    },
    {
      "content": "海の上に島があります。",
      "furigana": "[海|うみ]の[上|うえ]に[島|しま]があります。",
      "translation_zh_CN": "有海岛。"
    },
    {
      "content": "日本もイギリスも島国だ。",
      "furigana": "[日本|にっ|ぽん]もイギリスも[島国|しま|ぐに]だ。",
      "translation_zh_CN": "日本和英国都是岛国。"
    },
    {
      "content": "その島は日本の西方にある。",
      "furigana": "その[島|しま]は[日本|にっ|ぽん]の[西方|せい|ほう]にある。",
      "translation_zh_CN": "这个岛屿位于日本的西边。"
    },
    {
      "content": "島にはどれぐらい住んでたの？",
      "furigana": "[島|しま]にはどれぐらい[住|す]んでたの？",
      "translation_zh_CN": "你在岛上住了多久？"
    },
    {
      "content": "日本とイギリスは島国である。",
      "furigana": "[日本|に|ほん]とイギリスは[島国|しま|ぐに]である。",
      "translation_zh_CN": "日本和英国都是岛国。"
    },
    {
      "content": "島を出る手だてはありません。",
      "furigana": "[島|しま]を[出|で]る[手|て]だてはありません。",
      "translation_zh_CN": "没有办法离开这个岛。"
    },
    {
      "content": "その島には一軒しか店がない。",
